          SQLTask:Connection="{febbc687-d52e-400c-a8c0-9b5a06d1ae4e}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="---&#xA;--- Gestione tabella AULDG&#xA;---&#xA;&#xA;---&#xA;--- Crea tabella temporanea&#xA;---&#xA;  CREATE TABLE dbo.#temp_cf_auldg (sequenza int null,   cf varchar(16) null) on [primary];&#xA;&#xA;&#xA;---&#xA;--- Scarta i record con cf AUSCA anomali pone STATO LAVORAZIONE = -1&#xA;---&#xA; UPDATE TB_AULDG_PERSONA_GIURIDICA_LD&#xA; SET [AULDG_STATO_LAVORAZIONE] = -1&#xA; WHERE [AULDG_STATO_LAVORAZIONE] = 0 &#xA; AND ([AULDG_codice_fiscale] is null OR len([AULDG_codice_fiscale]) &gt; 11);&#xA;&#xA;---&#xA;--- Inserisce i record  nella tabella temporanea raggruppandoli per cf&#xA;---&#xA;    INSERT dbo.#temp_cf_auldg (sequenza, cf)&#xA;    SELECT MAX([auldg_sequenza_inserimento]), AULDG_codice_fiscale &#xA;&#x9;FROM TB_AULDG_PERSONA_GIURIDICA_LD&#xA;    WHERE [AULDG_STATO_LAVORAZIONE] = 0 &#xA;    GROUP BY AULDG_codice_fiscale;&#xA;&#xA;---&#xA;--- Scarto dei codici fiscali AUSCA duplicati pone STATO LAVORAZIONE = -2&#xA;---&#xA;&#x9;UPDATE TB_AULDG_PERSONA_GIURIDICA_LD&#xA;&#x9;SET [AULDG_STATO_LAVORAZIONE] = -2&#xA;&#x9;FROM TB_AULDG_PERSONA_GIURIDICA_LD INNER JOIN dbo.#temp_cf_auldg ON &#xA;&#x9; &#x9; [auldg_sequenza_inserimento] &lt;&gt; #temp_cf_auldg.[sequenza] &#xA;&#x9;AND AULDG_codice_fiscale = #temp_cf_auldg.cf&#xA;&#x9;WHERE [AULDG_STATO_LAVORAZIONE] = 0 ;&#xA;&#xA;---&#xA;--- Cancellazione tabella temporanea&#xA;---&#xA; DROP TABLE dbo.#temp_cf_auldg  ;&#xA;&#xA;---&#xA;--- Crea tabella temporanea&#xA;---&#xA;  CREATE TABLE dbo.#temp_cf2_auldg (codice_pk int null,   cf varchar(16) null) on [primary];&#xA;  &#xA;---&#xA;--- Scarta i record con cf RL anomali pone STATO LAVORAZIONE = -1&#xA;---&#xA;   UPDATE TB_AULDG_PERSONA_GIURIDICA_LD&#xA;   SET [AULDG_VALIDATO_AUSCO] = -1&#xA;   WHERE [AULDG_STATO_LAVORAZIONE] = 0 &#xA;&#x9;AND (&#xA;&#x9;&#x9;&#x9;[AULDG_codice_fiscale_rl] is null &#xA;&#x9;&#x9;&#x9;OR len([AULDG_codice_fiscale_rl]) &lt;&gt; 16 &#xA;&#x9;&#x9;&#x9;OR RTRIM(LTRIM([AULDG_codice_fiscale_rl])) = ''&#xA;&#x9;&#x9; ) ;&#xA;&#xA;---&#xA;--- Inserisce i record  nella tabella temporanea raggruppandoli per cf RL&#xA;---&#xA;    INSERT dbo.#temp_cf2_auldg (codice_pk, cf)&#xA;    SELECT MIN(AULDG_codice_pk), AULDG_codice_fiscale_rl &#xA;&#x9;FROM TB_AULDG_PERSONA_GIURIDICA_LD&#xA;    WHERE [AULDG_STATO_LAVORAZIONE] = 0 &#xA;&#x9;AND [AULDG_VALIDATO_AUSCO] = 0&#xA;    GROUP BY AULDG_codice_fiscale_rl ;&#xA;&#xA;---&#xA;--- Scarto dei codici fiscali rl duplicati pone VALIDATO AUSCO = -2&#xA;---&#xA;   UPDATE TB_AULDG_PERSONA_GIURIDICA_LD&#xA;   SET [AULDG_VALIDATO_AUSCO] = -2&#xA;   FROM TB_AULDG_PERSONA_GIURIDICA_LD INNER JOIN dbo.#temp_cf2_auldg ON &#xA;&#x9;&#x9;[AULDG_CODICE_PK] &lt;&gt; #temp_cf2_auldg.codice_pk &#xA;&#x9;AND AULDG_codice_fiscale_rl = #temp_cf2_auldg.cf&#xA;&#x9;WHERE [AULDG_STATO_LAVORAZIONE] = 0 &#xA;&#x9;AND [AULDG_VALIDATO_AUSCO] = 0 ;&#xA;&#xA;---&#xA;--- Cancellazione tabella temporanea&#xA;---&#xA; DROP TABLE #temp_cf2_auldg;&#xA;&#xA;---&#xA;--- Aggiornamento riferimenti esterni verso AUSCA &#xA;---&#xA;   UPDATE TB_AULDG_PERSONA_GIURIDICA_LD&#xA;   SET&#x9;[AULDG_VALIDATO_AUSCA] = 1, &#xA;&#x9;&#x9;[AULDG_FLAG_AUSCA] = 'S', &#xA;&#x9;&#x9;[AULDG_CODICE_AUSCA] = ausca_codice_pk&#xA;   FROM TB_AULDG_PERSONA_GIURIDICA_LD INNER JOIN tb_ausca_sog_contr_az ON &#xA;&#x9;&#x9;ausca_codice_fiscale = AULDG_codice_fiscale&#xA;   WHERE [AULDG_STATO_LAVORAZIONE] = 0  ;&#xA;&#xA;---&#xA;--- Aggiornamento riferimenti esterni verso AUSCO &#xA;---&#xA;   UPDATE TB_AULDG_PERSONA_GIURIDICA_LD&#xA;   SET&#x9;[AULDG_VALIDATO_AUSCO] = 1, &#xA;&#x9;&#x9;[AULDG_FLAG_AUSCO] = 'S', &#xA;&#x9;&#x9;[AULDG_CODICE_AUSCO] = ausco_codice_pk&#xA;   FROM TB_AULDG_PERSONA_GIURIDICA_LD INNER JOIN tb_ausco_sog_contr_col ON &#xA;&#x9;&#x9;ausco_codice_fiscale = AULDG_codice_fiscale_rl&#xA;   WHERE [AULDG_STATO_LAVORAZIONE] = 0 ;&#xA;&#xA;---&#xA;--- Aggiornamento riferimenti esterni verso AURSS&#xA;--- Se la relazione esiste non si farà nulla altrimenti si inserisce&#xA;   UPDATE TB_AULDG_PERSONA_GIURIDICA_LD&#xA;   SET&#x9;[AULDG_VALIDATO_AURSS] = 1, &#xA;&#x9;&#x9;[AULDG_FLAG_AURSS] = 'S', &#xA;&#x9;&#x9;[AULDG_CODICE_AURSS] = aurss_codice_pk&#xA;   FROM TB_AULDG_PERSONA_GIURIDICA_LD INNER JOIN tb_aurss_rel_sog_sog ON &#xA;&#x9;&#x9;AULDG_codice_ausca = aurss_ausca_codice_pk &#xA;&#x9;AND AULDG_codice_ausco = aurss_ausco_codice_pk &#xA;&#x9;AND aurss_autis_codice_pk = 35 &#xA;&#x9;AND [aurss_auten_codice_pk] = 3 &#xA;&#x9;AND [aurss_codice_entita_rif] = 6&#xA;&#x9;WHERE [AULDG_STATO_LAVORAZIONE] = 0 ;&#xA;&#xA;-- Inizio stored&#xA;&#xA;---&#xA;--- Procedura di aggiornamento indirizzi e toponimi&#xA;---&#xA;&#xA; --DECLARE CURSORE_INDIRIZZI Cursor&#xA; --FOR&#xA; --SELECT  [AULDG_indirizzo_sede_input], AULDG_codice_pk&#xA; -- FROM dbo.TB_AULDG_PERSONA_GIURIDICA_LD&#xA; -- WHERE ([AULDG_STATO_LAVORAZIONE] = 0) &#xA; -- AND [AULDG_indirizzo_sede_input] IS NOT NULL&#xA;       &#xA; --     OPEN CURSORE_INDIRIZZI &#xA;      &#xA; --    DECLARE @Indirizzo Varchar(255) &#xA; -- DECLARE @codice_pk int   &#xA; -- DECLARE @Nuovo_Indirizzo Varchar(255)    &#xA; -- DECLARE @Toponimo VARCHAR(50)&#xA; -- DECLARE @Codice_toponimo VARCHAR(3)&#xA; -- DECLARE @Ret_val INT&#xA;&#xA; --    FETCH NEXT FROM CURSORE_INDIRIZZI INTO @Indirizzo, @Codice_pk&#xA; -- WHILE (@@FETCH_STATUS = 0)  &#xA; -- BEGIN&#xA; --  SELECT @Nuovo_Indirizzo=Indirizzo, &#xA; --      @Toponimo=Toponimo, &#xA; --      @Codice_toponimo = Codice_toponimo, &#xA; --      @Ret_val = Ret_val FROM dbo.fn_indirizzo_e_toponimi(@Indirizzo)        &#xA; --  if @ret_val &lt;&gt; 0&#xA; --  BEGIN&#xA; --   UPDATE dbo.TB_AULDG_PERSONA_GIURIDICA_LD&#xA; --   SET [AULDG_indirizzo] = @nuovo_indirizzo,&#xA; --    [AULDG_toponimo]  = @toponimo,&#xA; --    [AULDG_codice_toponimo] = @codice_toponimo&#xA; --   WHERE AULDG_codice_pk = @Codice_pk&#xA; --  END &#xA; --  FETCH NEXT FROM CURSORE_INDIRIZZI INTO @Indirizzo, @Codice_pk&#xA; -- END&#xA; --CLOSE CURSORE_INDIRIZZI&#xA; --DEALLOCATE CURSORE_INDIRIZZI&#xA;&#xA;---&#xA;--- Aggiorna gli indirizzi senza toponimo&#xA;---&#xA; UPDATE dbo.TB_AULDG_PERSONA_GIURIDICA_LD&#xA; SET [AULDG_indirizzo] = [AULDG_indirizzo_sede_input]&#xA; WHERE  ([AULDG_STATO_LAVORAZIONE] = 0) &#xA; AND [AULDG_indirizzo_sede_input] IS NOT NULL &#xA; AND [AULDG_indirizzo] IS NULL ;&#xA;&#xA; --DECLARE CURSORE_INDIRIZZI Cursor&#xA; --FOR&#xA; --SELECT  [AULDG_indirizzo], AULDG_codice_pk&#xA; -- FROM dbo.TB_AULDG_PERSONA_GIURIDICA_LD&#xA; -- WHERE ([AULDG_STATO_LAVORAZIONE] = 0) &#xA; -- AND [AULDG_indirizzo] IS NOT NULL&#xA;       &#xA; --  OPEN CURSORE_INDIRIZZI &#xA;      &#xA; -- DECLARE @Indirizzo1 Varchar(255) &#xA; -- DECLARE @codice_pk1 int   &#xA; -- DECLARE @civico1 Varchar(50)    &#xA; -- DECLARE @posizione1 int&#xA; -- DECLARE @Ret_val1 INT&#xA;&#xA; -- FETCH NEXT FROM CURSORE_INDIRIZZI INTO @Indirizzo1, @Codice_pk1&#xA; -- WHILE (@@FETCH_STATUS = 0)  &#xA; -- BEGIN&#xA; --  SELECT @civico1=civico, @posizione1=pos_min, @Ret_val1 = Ret_val from  dbo.[fn_torna_civico](@Indirizzo1)        &#xA;&#xA; --  if @ret_val1 = 0&#xA; --   BEGIN&#xA; --    UPDATE dbo.TB_AULDG_PERSONA_GIURIDICA_LD&#xA; --    SET [AULDG_civico] = @civico1,&#xA; --    [AULDG_indirizzo]  = substring([AULDG_indirizzo],1, @posizione1-1)&#xA; --    WHERE AULDG_codice_pk = @Codice_pk1 and @Ret_val1 = 0&#xA; --  end &#xA;&#xA; --  FETCH NEXT FROM CURSORE_INDIRIZZI INTO @Indirizzo1, @Codice_pk1&#xA; -- END&#xA; --CLOSE CURSORE_INDIRIZZI&#xA; --DEALLOCATE CURSORE_INDIRIZZI&#xA;---&#xA;--- Tappi per campi indirizzo&#xA;---&#xA;&#xA; UPDATE TB_AULDG_PERSONA_GIURIDICA_LD&#xA; SET AULDG_toponimo = '--'&#xA; WHERE (AULDG_toponimo is null or RTRIM(LTRIM(AULDG_toponimo)) = '') &#xA; AND ([AULDG_STATO_LAVORAZIONE] = 0) ;&#xA;&#xA; UPDATE TB_AULDG_PERSONA_GIURIDICA_LD&#xA; SET AULDG_codice_toponimo = '--'&#xA; WHERE (AULDG_codice_toponimo is null or RTRIM(LTRIM(AULDG_codice_toponimo)) = '' ) &#xA; AND ([AULDG_STATO_LAVORAZIONE] = 0) ;&#xA;&#xA; UPDATE TB_AULDG_PERSONA_GIURIDICA_LD&#xA; SET AULDG_indirizzo = '--'&#xA; WHERE (AULDG_indirizzo is null or RTRIM(LTRIM(AULDG_indirizzo)) = '' ) &#xA; AND ([AULDG_STATO_LAVORAZIONE] = 0) ;&#xA;&#xA; UPDATE TB_AULDG_PERSONA_GIURIDICA_LD&#xA; SET AULDG_civico = 'SNC'&#xA; WHERE (AULDG_civico is null or RTRIM(LTRIM(AULDG_civico)) = '' ) &#xA; AND ([AULDG_STATO_LAVORAZIONE] = 0) ;&#xA;&#xA;--- ------------------------------------------------------------------------------------------&#xA;--- Elimina dall'indirizzo la virgola o il punto finale e la N finale precedeuta da spazio  --&#xA;--- ------------------------------------------------------------------------------------------&#xA; --UPDATE TB_AULDG_PERSONA_GIURIDICA_LD&#xA; --SET AULDG_indirizzo = substring(AULDG_indirizzo, 1, len(AULDG_indirizzo)-1)&#xA; --WHERE ( substring(AULDG_indirizzo,len(AULDG_indirizzo),1) = ',' &#xA; --  OR substring(AULDG_indirizzo,len(AULDG_indirizzo),1) = '.') &#xA; --  AND ([AULDG_STATO_LAVORAZIONE] = 0) &#xA; --  AND LEN(AULDG_indirizzo) &gt; 1 ;&#xA;&#xA; --UPDATE TB_AULDG_PERSONA_GIURIDICA_LD&#xA; --SET AULDG_indirizzo = substring(AULDG_indirizzo, 1, len(AULDG_indirizzo)-2)&#xA; --WHERE ( SUBSTRING(AULDG_indirizzo, LEN(AULDG_indirizzo),1) = 'N' &#xA; --  AND SUBSTRING(AULDG_indirizzo, LEN(AULDG_indirizzo)-1,1) = ' ') AND ([AULDG_STATO_LAVORAZIONE] = 0) &#xA; -- and LEN(AULDG_indirizzo) &gt; 2 ;&#xA;&#xA;---&#xA;--- Query di aggiornamento indice di qualità indirizzo da utilizzare per AUSCO&#xA;---&#xA;&#xA; UPDATE dbo.TB_AULDG_PERSONA_GIURIDICA_LD &#xA; SET auldg_indice_qualita_indirizzo = 0&#xA; WHERE ([AULDG_STATO_LAVORAZIONE] = 0) ;&#xA;&#xA;-- 1.1 : INDICE di QUALITA = 1  - INDIRIZZO + COMUNE + PROVINCIA &#xA; --UPDATE dbo.TB_AULDG_PERSONA_GIURIDICA_LD&#xA; --SET auldg_indice_qualita_indirizzo = 1&#xA; --WHERE ([AULDG_STATO_LAVORAZIONE] = 0) and&#xA; --( ISNULL(AULDG_indirizzo, '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDG_indirizzo)) &lt;&gt; '' AND  LTRIM(RTRIM(AULDG_indirizzo)) &lt;&gt; '-' AND  LTRIM(RTRIM(AULDG_indirizzo)) &lt;&gt; '--') &#xA; --AND ( ISNULL(AULDG_comune,  '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDG_comune )) &lt;&gt; '' AND  LTRIM(RTRIM(AULDG_comune))  &lt;&gt; '-' AND  LTRIM(RTRIM(AULDG_comune))  &lt;&gt; '--') &#xA; --AND ( ISNULL(AULDG_sigla_provincia, '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDG_sigla_provincia)) &lt;&gt; '' AND  LTRIM(RTRIM(AULDG_sigla_provincia)) &lt;&gt; '-' AND  LTRIM(RTRIM(AULDG_sigla_provincia)) &lt;&gt; '--');&#xA;&#xA;&#xA;-- 1.2 : INDICE di QUALITA = 2  - INDIRIZZO + COMUNE + PROVINCIA + CIVICO + TOPONIMO&#xA; --UPDATE dbo.TB_AULDG_PERSONA_GIURIDICA_LD&#xA; --SET auldg_indice_qualita_indirizzo = 2&#xA; --WHERE ([AULDG_STATO_LAVORAZIONE] = 0) and&#xA; --( ISNULL(AULDG_indirizzo, '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDG_indirizzo)) &lt;&gt; '' AND  LTRIM(RTRIM(AULDG_indirizzo)) &lt;&gt; '-' AND  LTRIM(RTRIM(AULDG_indirizzo)) &lt;&gt; '--') &#xA; --AND ( ISNULL(AULDG_comune,  '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDG_comune )) &lt;&gt; '' AND  LTRIM(RTRIM(AULDG_comune))  &lt;&gt; '-' AND  LTRIM(RTRIM(AULDG_comune))  &lt;&gt; '--') &#xA; --AND ( ISNULL(AULDG_sigla_provincia, '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDG_sigla_provincia)) &lt;&gt; '' AND  LTRIM(RTRIM(AULDG_sigla_provincia)) &lt;&gt; '-' AND  LTRIM(RTRIM(AULDG_sigla_provincia)) &lt;&gt; '--')&#xA; --AND ( ISNULL(AULDG_civico,   '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDG_civico)) &lt;&gt; '' AND  LTRIM(RTRIM(AULDG_civico)) &lt;&gt; '-' AND  LTRIM(RTRIM(AULDG_civico))    &lt;&gt; '--')&#xA; --AND ( ISNULL(AULDG_codice_toponimo, '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDG_codice_toponimo))   &lt;&gt; '' AND  LTRIM(RTRIM(AULDG_codice_toponimo))  &lt;&gt; '-' AND  LTRIM(RTRIM(AULDG_codice_toponimo))  &lt;&gt; '--');&#xA;&#xA;-- 1.3 : INDICE di QUALITA = 3  - INDIRIZZO + COMUNE + PROVINCIA + CIVICO + TOPONIMO + CAP&#xA;&#xA; --UPDATE dbo.TB_AULDG_PERSONA_GIURIDICA_LD&#xA; --SET auldg_indice_qualita_indirizzo = 3&#xA; --WHERE ([AULDG_STATO_LAVORAZIONE] = 0) and&#xA; --( ISNULL(AULDG_indirizzo, '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDG_indirizzo)) &lt;&gt; '' AND  LTRIM(RTRIM(AULDG_indirizzo)) &lt;&gt; '-' AND  LTRIM(RTRIM(AULDG_indirizzo)) &lt;&gt; '--') &#xA; --AND ( ISNULL(AULDG_comune,  '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDG_comune )) &lt;&gt; '' AND  LTRIM(RTRIM(AULDG_comune))  &lt;&gt; '-' AND  LTRIM(RTRIM(AULDG_comune))  &lt;&gt; '--') &#xA; --AND ( ISNULL(AULDG_sigla_provincia, '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDG_sigla_provincia)) &lt;&gt; '' AND  LTRIM(RTRIM(AULDG_sigla_provincia)) &lt;&gt; '-' AND  LTRIM(RTRIM(AULDG_sigla_provincia)) &lt;&gt; '--')&#xA; --AND ( ISNULL(AULDG_civico,   '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDG_civico)) &lt;&gt; '' AND  LTRIM(RTRIM(AULDG_civico)) &lt;&gt; '-' AND  LTRIM(RTRIM(AULDG_civico))    &lt;&gt; '--')&#xA; --AND ( ISNULL(AULDG_codice_toponimo, '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDG_codice_toponimo))   &lt;&gt; '' AND  LTRIM(RTRIM(AULDG_codice_toponimo))  &lt;&gt; '-' AND  LTRIM(RTRIM(AULDG_codice_toponimo))  &lt;&gt; '--')&#xA; --AND ( ISNULL(AULDG_cap,'---') &lt;&gt; '---'  AND LTRIM(RTRIM(AULDG_cap)) &lt;&gt; '' AND  LTRIM(RTRIM(AULDG_cap)) &lt;&gt; '' AND LTRIM(RTRIM(AULDG_cap)) &lt;&gt; '00000' ) ;&#xA;&#xA;&#xA;-- Fine stored&#xA;&#xA;&#xA;---&#xA;--- Inserimento su AUSCA&#xA;---&#xA;&#xA;INSERT INTO [dbo].[tb_ausca_sog_contr_az] (&#xA;  [ausca_codice_fiscale]&#xA;            ,[ausca_denominazione]&#xA;            ,[ausca_cap]&#xA;            ,[ausca_descr_comune]&#xA;            ,[ausca_sigla_provincia]&#xA;            ,[ausca_PEC]&#xA;&#x9;&#x9;&#x9;,[ausca_email]&#xA;            ,[ausca_telefono1]&#xA;            ,[ausca_toponimo]&#xA;            ,[ausca_codice_toponimo]&#xA;            ,[ausca_indirizzo]&#xA;            ,[ausca_civico]&#xA;            ,[ausca_codice_qualita_ind]&#xA;            ,[ausca_sede_legale_italia]&#xA;            ,[ausca_soggetto_certificato]&#xA;            ,[ausca_auten_codice_pk]&#xA;            ,[ausca_codice_entita_rif]&#xA;&#x9;&#x9;&#x9;,[ausca_aungi_codice_pk]&#xA;            ,[ausca_note]&#xA;            ,[ausca_data_modifica]&#xA;            ,[ausca_descr_utente] )&#xA; (&#xA;&#x9;SELECT UPPER([AULDG_codice_fiscale])&#xA;            ,[AULDG_denominazione]&#xA;            ,[AULDG_cap]&#xA;            ,LEFT([AULDG_comune],30)&#xA;            ,[AULDG_sigla_provincia]&#xA;            ,[AULDG_pec]&#xA;            ,[AULDG_email]&#xA;            ,[AULDG_telefono]&#xA;            ,[AULDG_toponimo]&#xA;            ,[AULDG_codice_toponimo]&#xA;            ,[AULDG_indirizzo]&#xA;            ,[AULDG_civico]&#xA;            ,[auldg_indice_qualita_indirizzo] &#xA;&#x9;&#x9;&#x9;,'S' as sedeItalia&#xA;&#x9;&#x9;&#x9;,'N' as Certificato&#xA;&#x9;&#x9;&#x9;, 3 as tipoEntita&#xA;&#x9;&#x9;&#x9;, 6 as Applicazione_LD&#xA;&#x9;&#x9;&#x9;, 57 as AUNGI&#xA;&#x9;&#x9;&#x9;,'Proveniente da Lavoratori Domestici PG' as Note&#xA;&#x9;&#x9;&#x9;,GETDATE() as DataModifica&#xA;&#x9;&#x9;&#x9;,'Lavoratori Domestici PG' as DescrUtente&#xA;&#x9;FROM [dbo].TB_AULDG_PERSONA_GIURIDICA_LD&#xA;&#x9;WHERE [AULDG_STATO_LAVORAZIONE] = 0 &#xA;&#x9;AND [AULDG_VALIDATO_AUSCA] = 0 &#xA; );&#xA;&#xA;&#xA;---&#xA;--- Modifica su AUSCA solo se non è certificato e proviene da LD&#xA;---&#xA;&#xA;UPDATE [dbo].TB_AULDG_PERSONA_GIURIDICA_LD&#xA;SET [auldg_denominazione] = NULL&#xA;WHERE RTRIM(LTRIM([auldg_denominazione] )) = '';&#xA;&#xA;UPDATE [dbo].TB_AULDG_PERSONA_GIURIDICA_LD&#xA;SET [auldg_pec] = NULL&#xA;WHERE rtrim(ltrim([auldg_pec] )) = '';&#xA;&#xA;UPDATE [dbo].TB_AULDG_PERSONA_GIURIDICA_LD&#xA;SET [auldg_email] = NULL&#xA;WHERE rtrim(ltrim([auldg_email] )) = '';&#xA;&#xA;UPDATE [dbo].TB_AULDG_PERSONA_GIURIDICA_LD&#xA;SET [auldg_telefono] = NULL&#xA;WHERE rtrim(ltrim([auldg_telefono] )) = '';&#xA;&#xA;UPDATE [dbo].[tb_ausca_sog_contr_az]&#xA;SET &#xA;     [ausca_denominazione] = ISNULL(left([auldg_denominazione],30),[ausca_denominazione] )&#xA;       ,[ausca_PEC] = ISNULL([auldg_pec],[ausca_PEC] )&#xA;       ,[ausca_email] = ISNULL([auldg_email],[ausca_email] )&#xA;       ,[ausca_telefono1] = ISNULL([auldg_telefono],[ausca_telefono1] )&#xA;       ,[ausca_data_modifica] = getdate()&#xA;       ,[ausca_descr_utente] = 'Lavoratori Domestici PG'&#xA;&#xA;FROM [tb_ausca_sog_contr_az] INNER JOIN [dbo].TB_AULDG_PERSONA_GIURIDICA_LD ON &#xA;  [tb_ausca_sog_contr_az].ausca_codice_pk = TB_AULDG_PERSONA_GIURIDICA_LD.[AULDG_CODICE_AUSCA]&#xA;WHERE [AULDG_STATO_LAVORAZIONE] = 0 &#xA;AND [AULDG_VALIDATO_AUSCA] = 1 &#xA;AND [AULDG_FLAG_AUSCA] = 'S' &#xA;AND ausca_soggetto_certificato = 'N' &#xA;AND [ausca_auten_codice_pk] = 3 &#xA;AND [ausca_codice_entita_rif] = 6;&#xA;&#xA;---&#xA;--- Modifica indirizzo su AUSCA solo se non è certificato e proviene da LD e il codice di qualità&#xA;--- entrante è maggiore di quello presente&#xA;---&#xA;&#xA;UPDATE [dbo].[tb_ausca_sog_contr_az]&#xA;SET &#xA;         [ausca_cap] = [auldg_cap]&#xA;        ,[ausca_descr_comune] =  case isnull(auldg_comune, '--') when '--' then null else left(auldg_comune,30) end&#xA;        ,[ausca_sigla_provincia] = [auldg_sigla_provincia]&#xA;        ,[ausca_toponimo] = [auldg_toponimo]&#xA;        ,[ausca_codice_toponimo] = [auldg_codice_toponimo]&#xA;        ,[ausca_indirizzo] = [auldg_indirizzo]&#xA;        ,[ausca_civico] = [auldg_civico]&#xA;        ,[ausca_codice_qualita_ind] = [auldg_indice_qualita_indirizzo]&#xA;        ,[ausca_data_modifica] = getdate()&#xA;        ,[ausca_descr_utente] = 'Lavoratori Domestici PG'&#xA;FROM [tb_ausca_sog_contr_az] inner join [dbo].TB_AULDG_PERSONA_GIURIDICA_LD ON &#xA;  [tb_ausca_sog_contr_az].ausca_codice_pk = TB_AULDG_PERSONA_GIURIDICA_LD.[AULDG_CODICE_AUSCA]&#xA;WHERE [AULDG_STATO_LAVORAZIONE] = 0 &#xA;AND [AULDG_VALIDATO_AUSCA] = 1 &#xA;AND [AULDG_FLAG_AUSCA] = 'S' &#xA;AND ausca_soggetto_certificato = 'N' &#xA;AND [ausca_auten_codice_pk] = 3 &#xA;AND [ausca_codice_entita_rif] = 6 &#xA;AND [auldg_indice_qualita_indirizzo] &gt; ausca_codice_qualita_ind;&#xA;&#xA;-------------- nuovo&#xA;&#xA;&#xA;---&#xA;--- Pone AULDG_VALIDATO_AUSCA = 2 per i soli record appena inseriti: servirà per aucfi e le variazioni storico ed aurss&#xA;---&#xA;&#xA; UPDATE  dbo.TB_AULDG_PERSONA_GIURIDICA_LD&#xA; SET AULDG_VALIDATO_AUSCA = 2, &#xA;  AULDG_FLAG_AUSCA = 'S', &#xA;  AULDG_CODICE_AUSCA = ausca_codice_pk&#xA; FROM TB_AULDG_PERSONA_GIURIDICA_LD INNER JOIN tb_ausca_sog_contr_az ON &#xA;   TB_AULDG_PERSONA_GIURIDICA_LD.AULDG_codice_fiscale = tb_ausca_sog_contr_az.ausca_codice_fiscale&#xA; WHERE AULDG_stato_lavorazione = 0 &#xA; AND [AULDG_VALIDATO_AUSCA] = 0 ;&#xA;&#xA;---&#xA;--- Inserimento su AUSCO&#xA;---&#xA; INSERT INTO [dbo].[tb_ausco_sog_contr_col] (&#xA;   [ausco_codice_fiscale]&#xA;  ,[ausco_cognome]&#xA;  ,[ausco_nome]&#xA;  ,[ausco_sesso]&#xA;  ,[ausco_comune_nascita]&#xA;  ,[ausco_data_nascita]&#xA;  ,[ausco_prov_nascita]&#xA;  ,[ausco_note]&#xA;  ,[ausco_tipo_persona]&#xA;  ,[ausco_data_modifica]&#xA;  ,[ausco_descr_utente]&#xA;  ,[ausco_aurea_codice_pk] )&#xA;  (&#xA;&#x9;SELECT  &#xA;&#x9;&#x9;UPPER([AULDG_codice_fiscale_rl])&#xA;&#x9;&#x9;,[AULDG_cognome_rl]&#xA;&#x9;&#x9;,[AULDG_nome_rl]&#xA;&#x9;&#x9;,[AULDG_sesso_rl]&#xA;&#x9;&#x9;,[AULDG_comune_nascita_rl]&#xA;&#x9;&#x9;,[AULDG_data_nascita_rl]&#xA;&#x9;&#x9;,[AULDG_prov_nascita_rl]&#xA;&#x9;&#x9;,'Proveniente da Lavoratori Domestici PG' as Note&#xA;&#x9;&#x9;--,'G' as TipoPersona&#xA;&#x9;&#x9;, CASE LEN(LTRIM(RTRIM(AULDG_codice_fiscale_rl))) WHEN 16 THEN 'F' ELSE 'G' END As TipoPersona&#xA;&#x9;&#x9;,getdate() as DataModifica&#xA;&#x9;&#x9;,'Lavoratori Domestici PG' as DescrUtente&#xA;&#x9;&#x9;,11 as Aurea&#xA;&#x9;FROM [dbo].TB_AULDG_PERSONA_GIURIDICA_LD&#xA;&#x9;WHERE [AULDG_STATO_LAVORAZIONE] = 0 &#xA;&#x9;AND [AULDG_VALIDATO_AUSCO] = 0 &#xA; ) ;&#xA;&#xA;---&#xA;--- Modifica su AUSCO solo se proviene da LD&#xA;---&#xA;&#xA;UPDATE [dbo].TB_AULDG_PERSONA_GIURIDICA_LD &#xA;SET [auldg_cognome_rl] = NULL&#xA;WHERE RTRIM(LTRIM([auldg_cognome_rl] )) = '';&#xA;&#xA;UPDATE [dbo].TB_AULDG_PERSONA_GIURIDICA_LD &#xA;SET [auldg_nome_rl] = NULL&#xA;WHERE RTRIM(LTRIM([auldg_nome_rl] )) = '';&#xA;&#xA;UPDATE [dbo].TB_AULDG_PERSONA_GIURIDICA_LD &#xA;SET [auldg_sesso_rl] = NULL&#xA;WHERE RTRIM(LTRIM([auldg_sesso_rl] )) = '';&#xA;&#xA;UPDATE [dbo].TB_AULDG_PERSONA_GIURIDICA_LD &#xA;SET [auldg_comune_nascita_rl] = NULL&#xA;WHERE RTRIM(LTRIM([auldg_comune_nascita_rl])) = '';&#xA;&#xA;UPDATE [dbo].TB_AULDG_PERSONA_GIURIDICA_LD &#xA;SET [auldg_data_nascita_rl] = NULL&#xA;WHERE RTRIM(LTRIM([auldg_data_nascita_rl] )) = '';&#xA;&#xA;UPDATE [dbo].TB_AULDG_PERSONA_GIURIDICA_LD &#xA;SET [auldg_prov_nascita_rl] = NULL&#xA;WHERE RTRIM(LTRIM([auldg_prov_nascita_rl] )) = '';&#xA;&#xA;UPDATE [tb_ausco_sog_contr_col]&#xA;SET  [ausco_cognome] = ISNULL([auldg_cognome_rl], [ausco_cognome] )&#xA;    ,[ausco_nome] = ISNULL([auldg_nome_rl], [ausco_nome] )&#xA;    ,[ausco_sesso] = ISNULL([auldg_sesso_rl], [ausco_sesso] )&#xA;    ,[ausco_comune_nascita] = ISNULL([auldg_comune_nascita_rl], [ausco_comune_nascita] )&#xA;    ,[ausco_data_nascita] = ISNULL([auldg_data_nascita_rl],[ausco_data_nascita])&#xA;    ,[ausco_prov_nascita] = ISNULL([auldg_prov_nascita_rl],[ausco_prov_nascita])&#xA;    ,[ausco_data_modifica] = getdate()&#xA;    ,[ausco_descr_utente] = 'Lavoratori Domestici PG'&#xA;FROM [tb_ausco_sog_contr_col] INNER JOIN TB_AULDG_PERSONA_GIURIDICA_LD ON &#xA; ausco_codice_pk = [AULDG_CODICE_AUSCO]&#xA;WHERE [ausco_aurea_codice_pk] = 11 &#xA;AND [AULDG_STATO_LAVORAZIONE] = 0 &#xA;AND [AULDG_VALIDATO_AUSCO] = 1 &#xA;AND [AULDG_FLAG_AUSCO] = 'S';&#xA;&#xA;---&#xA;--- Modifica indirizzo su AUSCO solo se proviene da LD e il codice di qualità entrante &#xA;--- è maggiore di quello presente&#xA;---&#xA;--UPDATE [tb_ausco_sog_contr_col]&#xA;--SET          [ausco_codice_toponimo] = [auldf_codice_toponimo]&#xA;--            ,[ausco_toponimo] = [auldf_toponimo]&#xA;--            ,[ausco_localita] = [auldf_comune]&#xA;--            ,[ausco_indirizzo] = [auldf_indirizzo]&#xA;--            ,[ausco_civico] = [auldf_civico]&#xA;--            ,[ausco_cap] = [auldf_cap]&#xA;--            ,[ausco_provincia] = [auldf_sigla_provincia]&#xA;--            ,[ausco_codice_qualita_ind] = [auldf_indice_qualita_indirizzo]&#xA;--            ,[ausco_ind_qualita_indir] = [auldf_indice_qualita_indirizzo]&#xA;--            ,[ausco_data_modifica] = getdate()&#xA;--            ,[ausco_descr_utente] = 'Lavoratori Domestici PF'&#xA;--from [tb_ausco_sog_contr_col] inner join [TB_AULDF_PERSONA_FISICA_LD] ON ausco_codice_pk = [AULDF_CODICE_AUSCO]&#xA;--WHERE [ausco_aurea_codice_pk] = 11 and [AULDF_STATO_LAVORAZIONE] = 0 and [AULDF_VALIDATO_AUSCO] = 1 and [AULDF_FLAG_AUSCO] = 'S' and &#xA;--[auldf_indice_qualita_indirizzo] &gt; [ausco_ind_qualita_indir]&#xA;&#xA;---&#xA;--- Pone AULDG_VALIDATO_AUSCO = 2 per i soli record appena inseriti: servirà per aurss&#xA;---&#xA;&#xA; UPDATE  TB_AULDG_PERSONA_GIURIDICA_LD&#xA; SET AULDG_VALIDATO_AUSCO = 2, &#xA;  AULDG_FLAG_AUSCO = 'S', &#xA;  AULDG_CODICE_AUSCO = ausco_codice_pk&#xA; FROM TB_AULDG_PERSONA_GIURIDICA_LD INNER JOIN tb_ausco_sog_contr_col ON &#xA;   TB_AULDG_PERSONA_GIURIDICA_LD.AULDG_codice_fiscale_rl = tb_ausco_sog_contr_col.ausco_codice_fiscale&#xA; WHERE AULDG_stato_lavorazione = 0 &#xA; AND [AULDG_VALIDATO_AUSCO] = 0;&#xA;&#xA;---&#xA;--- Inserimento Relazioni&#xA;---&#xA;&#xA; INSERT INTO [dbo].[tb_aurss_rel_sog_sog] (&#xA;  [aurss_ausca_codice_pk]&#xA;            ,[aurss_ausco_codice_pk]&#xA;            ,[aurss_aussu_codice_pk]&#xA;            ,[aurss_autis_codice_pk]&#xA;            ,[aurss_data_inizio_validita]&#xA;            ,[aurss_data_di_fine_validita]&#xA;            ,[aurss_note]&#xA;            ,[aurss_rappresentante_legale]&#xA;            ,[aurss_data_modifica]&#xA;            ,[aurss_descr_utente]&#xA;            ,[aurss_auten_codice_pk]&#xA;            ,[aurss_codice_entita_rif]&#xA;            ,[aurss_codice_carica]&#xA;            ,[aurss_provenienza] )&#xA; (&#xA; SELECT  [AULDG_CODICE_AUSCA]&#xA;            ,[AULDG_CODICE_AUSCO]&#xA;  ,1 as StatoSoggetto&#xA;  ,35 as TipoSoggetto&#xA;  ,'1900-01-01'&#xA;  ,'2999-12-31'&#xA;  ,'Proveniente da Lavoratori Domestici PG' as Note&#xA;  ,'S' as RappLegale&#xA;  ,getdate() as DataModifica&#xA;  ,'Lavoratori Domestici PG' as DescrUtente&#xA;  ,3 as tipoEntita&#xA;  ,6 as Applicazione_LD&#xA;  ,'LER' as CodiceCarica&#xA;  ,'Lavoratori Domestici PG' as provenienza&#xA; from [dbo].TB_AULDG_PERSONA_GIURIDICA_LD&#xA; WHERE [AULDG_STATO_LAVORAZIONE] = 0 and [AULDG_VALIDATO_AURSS] = 0 &#xA; and AULDG_codice_ausca &lt;&gt; 0 and AULDG_codice_ausco &lt;&gt; 0&#xA; );&#xA;&#xA;&#xA;---&#xA;--- Aggiornamento AUCFI&#xA;--- Aggiornamento storico&#xA;--- Creazione controcodice&#xA;---&#xA;&#xA; DECLARE CURSORE_AUCFI Cursor&#xA; FOR&#xA; SELECT  upper([AULDG_codice_fiscale]), AULDG_codice_ausca&#xA;  from dbo.TB_AULDG_PERSONA_GIURIDICA_LD&#xA;  WHERE ([AULDG_STATO_LAVORAZIONE] = 0) and AULDG_VALIDATO_AUSCA = 2&#xA;       &#xA;      OPEN CURSORE_AUCFI &#xA;     Declare @CF_AUCFI Varchar(16) &#xA;  Declare @codice_pk_AUCFI int   &#xA;      FETCH NEXT FROM CURSORE_AUCFI INTO @CF_AUCFI, @codice_pk_AUCFI&#xA; WHILE (@@FETCH_STATUS = 0)  &#xA; BEGIN&#xA;  INSERT INTO [AUC].[dbo].[tb_aucfi_cod_fiscale] (&#xA;   [aucfi_ausca_codice_pk]&#xA;   ,[aucfi_data_inizio_validita]&#xA;   ,[aucfi_data_fine_validita]&#xA;   ,[aucfi_codice_fiscale]&#xA;   ,[aucfi_data_modifica]&#xA;   ,[aucfi_descr_utente]&#xA;   ,[aucfi_codice_pk_db2]&#xA;   ,[aucfi_rec_del]&#xA;   ,[aucfi_data_del] )&#xA;  VALUES(@codice_pk_AUCFI, GETDATE(), null, upper(@CF_AUCFI), GETDATE(), 'Proveniente da Lavoratori Domestici PG', 0, null, null) &#xA;&#xA;  EXEC AZ_VALIDA_CC @codice_pk_AUCFI, 'AUSCA'; &#xA;&#xA;  FETCH NEXT FROM CURSORE_AUCFI INTO @CF_AUCFI, @codice_pk_AUCFI&#xA; END&#xA; CLOSE CURSORE_AUCFI&#xA; DEALLOCATE CURSORE_AUCFI&#xA;&#xA;--storico&#xA;&#xA; DECLARE CURSORE_AUVAR Cursor&#xA; FOR&#xA; SELECT  upper([AULDG_codice_fiscale]), AULDG_codice_ausca&#xA;  from dbo.TB_AULDG_PERSONA_GIURIDICA_LD&#xA;  WHERE ([AULDG_STATO_LAVORAZIONE] = 0) and AULDG_VALIDATO_AUSCA &gt;= 0 and AULDG_FLAG_AUSCA = 'S'&#xA;       &#xA;      OPEN CURSORE_AUVAR &#xA;     Declare @CF_AUVAR Varchar(16) &#xA; Declare @codice_pk_AUVAR int   &#xA;      FETCH NEXT FROM CURSORE_AUVAR INTO @CF_AUVAR, @Codice_pk_AUVAR&#xA; WHILE (@@FETCH_STATUS = 0)  &#xA; BEGIN&#xA;  EXEC AZ_1_SINCRONIZZA @Codice_pk_AUVAR, '3', '6'; &#xA;&#xA;  FETCH NEXT FROM CURSORE_AUVAR INTO @CF_AUVAR, @Codice_pk_AUVAR&#xA; END&#xA; CLOSE CURSORE_AUVAR&#xA; DEALLOCATE CURSORE_AUVAR&#xA;&#xA;-- Fine" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
