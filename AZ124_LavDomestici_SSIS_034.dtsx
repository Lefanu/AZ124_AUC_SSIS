          SQLTask:Connection="{febbc687-d52e-400c-a8c0-9b5a06d1ae4e}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="--- Modificato il 27 febbraio 2018 per gestire lo stato di Sospesa&#xA;&#xA;---&#xA;--- Gestione tabella AULDR&#xA;---&#xA;--- Crea tabella temporanea&#xA;---&#xA;CREATE TABLE dbo.#temp_progr (sequenza int null,   progressivo varchar(20) null) on [primary];&#xA;&#xA;---&#xA;--- Scarta i record con cf anomali pone STATO LAVORAZIONE = -1&#xA;---&#xA;UPDATE TB_AULDR_RAPPORTI_LD&#xA;SET [AULDR_STATO_LAVORAZIONE] = -1&#xA;WHERE [AULDR_STATO_LAVORAZIONE] = 0 &#xA;AND ([AULDR_codice_fiscale] is null OR len([AULDR_codice_fiscale]) &gt; 16 &#xA;OR ( LEN(LTRIM(RTRIM([AULDR_codice_fiscale]))) &gt; 11 AND &#xA;&#x9; LEN(LTRIM(RTRIM([AULDR_codice_fiscale]))) &lt; 16)&#xA;&#x9;);&#xA;---&#xA;--- Inserisce i record  nella tabella temporanea raggruppANDoli per progressivo rappORto&#xA;---&#xA;INSERT #temp_progr (sequenza, progressivo)&#xA;&#xA;SELECT MAX([auldr_sequenza_inserimento]), auldr_progressivo_rapporto &#xA;FROM TB_AULDR_RAPPORTI_LD&#xA;WHERE [AULDr_STATO_LAVORAZIONE] = 0 &#xA;GROUP BY auldr_progressivo_rapporto;&#xA;&#xA;---&#xA;--- Scarto dei progressivi duplicati pone STATO LAVORAZIONE = -2&#xA;---&#xA;UPDATE TB_AULDR_RAPPORTI_LD&#xA;SET [AULDR_STATO_LAVORAZIONE] = -2&#xA;FROM TB_AULDR_RAPPORTI_LD INNER JOIN #temp_progr ON &#xA;&#x9;[auldr_sequenza_inserimento] &lt;&gt; #temp_progr.[sequenza] &#xA;AND auldr_progressivo_rapporto = #temp_progr.progressivo&#xA;WHERE [AULDr_STATO_LAVORAZIONE] = 0 ;&#xA;&#xA;---&#xA;--- Cancellazione tabella temporanea&#xA;---&#xA;DROP TABLE #temp_progr ;&#xA;&#xA;---&#xA;--- Aggiornamento riferimenti esterni verso AUSCA &#xA;---&#xA;UPDATE  TB_AULDR_RAPPORTI_LD&#xA;SET &#xA;  [AULDR_VALIDATO_AUSCA] = 1, &#xA;  [AULDR_FLAG_AUSCA] = 'S', &#xA;  [AULDR_CODICE_AUSCA] = ausca_codice_pk&#xA;FROM TB_AULDR_RAPPORTI_LD INNER JOIN tb_ausca_sog_contr_az ON &#xA;     ausca_codice_fiscale = AULDR_codice_fiscale&#xA;WHERE [AULDR_STATO_LAVORAZIONE] = 0 ;&#xA;&#xA;---&#xA;--- Aggiornamento riferimenti esterni verso AUPOC &#xA;---&#xA; UPDATE  TB_AULDR_RAPPORTI_LD&#xA; SET &#xA;  [AULDR_VALIDATO_AUPOC] = 1, &#xA;  [AULDR_FLAG_AUPOC] = 'S', &#xA;  [AULDR_CODICE_AUPOC] = aupoc_codice_pk&#xA; FROM TB_AULDR_RAPPORTI_LD INNER JOIN tb_aupoc_pos_contr ON &#xA;       aupoc_posizione = AULDR_progressivo_rapporto&#xA; WHERE [AULDR_STATO_LAVORAZIONE] = 0 &#xA; AND aupoc_aurea_codice_pk = 11 ;&#xA;&#xA;---&#xA;--- Aggiornamento denominazione per persone fisiche&#xA;---&#xA; UPDATE TB_AULDR_RAPPORTI_LD&#xA; SET auldr_denominazione = substring(auldf_cognome,1,len(TB_AULDF_PERSONA_FISICA_LD.auldf_cognome)) + ' ' + substring(auldf_nome,1,len(TB_AULDF_PERSONA_FISICA_LD.auldf_nome))&#xA; FROM TB_AULDR_RAPPORTI_LD INNER JOIN TB_AULDF_PERSONA_FISICA_LD ON &#xA;      TB_AULDR_RAPPORTI_LD.auldr_codice_fiscale = TB_AULDF_PERSONA_FISICA_LD.auldf_codice_fiscale&#xA; WHERE (TB_AULDR_RAPPORTI_LD.[AULDR_STATO_LAVORAZIONE] = 0) &#xA; AND (TB_AULDF_PERSONA_FISICA_LD.AULDF_STATO_LAVORAZIONE = 0) ;&#xA;&#xA;---&#xA;--- Aggiornamento denominazione per persone giuridiche &#xA;---&#xA;&#xA;UPDATE TB_AULDR_RAPPORTI_LD&#xA;SET auldr_denominazione = TB_AULDG_PERSONA_GIURIDICA_LD.auldg_denominazione&#xA;FROM TB_AULDR_RAPPORTI_LD INNER JOIN TB_AULDG_PERSONA_GIURIDICA_LD ON &#xA;     TB_AULDR_RAPPORTI_LD.auldr_codice_fiscale = TB_AULDG_PERSONA_GIURIDICA_LD.auldg_codice_fiscale&#xA;WHERE (TB_AULDR_RAPPORTI_LD.AULDR_STATO_LAVORAZIONE = 0) &#xA;AND (TB_AULDG_PERSONA_GIURIDICA_LD.AULDg_STATO_LAVORAZIONE = 0) ;&#xA;&#xA;---&#xA;--- Aggiorna gli indirizzi senza toponimo&#xA;---&#xA; UPDATE dbo.TB_AULDR_RAPPORTI_LD&#xA; SET [AULDR_indirizzo] = [AULDR_indirizzo_input]&#xA; WHERE        ([AULDR_STATO_LAVORAZIONE] = 0) &#xA; AND [AULDR_indirizzo_input] is not null &#xA; AND AULDR_indirizzo is null ;&#xA;&#xA;---&#xA;--- Tappi per campi indirizzo&#xA;---&#xA; UPDATE TB_AULDR_RAPPORTI_LD&#xA; SET AULDR_toponimo = '--'&#xA; WHERE (AULDR_toponimo is null &#xA; OR rtrim(ltrim(AULDR_toponimo)) = '') &#xA; AND ([AULDR_STATO_LAVORAZIONE] = 0);&#xA;&#xA; UPDATE TB_AULDR_RAPPORTI_LD&#xA; SET AULDR_codice_toponimo = '--'&#xA; WHERE (AULDR_codice_toponimo is null &#xA; OR rtrim(ltrim(AULDR_codice_toponimo)) = '') &#xA; AND ([AULDR_STATO_LAVORAZIONE] = 0);&#xA;&#xA; UPDATE TB_AULDR_RAPPORTI_LD&#xA; SET AULDR_indirizzo = '--'&#xA; WHERE (AULDR_indirizzo is null &#xA; OR rtrim(ltrim(AULDR_indirizzo)) = '') &#xA; AND ([AULDR_STATO_LAVORAZIONE] = 0);&#xA;&#xA; UPDATE TB_AULDR_RAPPORTI_LD&#xA; SET AULDR_civico = 'SNC'&#xA; WHERE (AULDR_civico is null &#xA; OR rtrim(ltrim(AULDR_civico)) = '') &#xA; AND ([AULDR_STATO_LAVORAZIONE] = 0);&#xA;&#xA;---&#xA;--- Query di aggiornamento indice di qualità indirizzo da utilizzare per AUIND&#xA;---&#xA; UPDATE dbo.TB_AULDR_RAPPORTI_LD &#xA; SET auldr_indice_qualita_indirizzo = 0&#xA; WHERE ([AULDR_STATO_LAVORAZIONE] = 0) ;&#xA;&#xA;--- DETERMINAZIONE DELLO STATO DELLA POSIZIONE &#xA;-- 1 -- Modificato il 27 febbraio 2018&#xA;-- Stato Pratica = 1 = Accolta diventa stato Attiva (= 1)&#xA;-- Stato Pratica = 9 = Annullata diventa stato Annullata (= 7)&#xA;-- Stato Pratica = 2 = Respinta diventa stato Respinta (= 8)&#xA;-- Stato Pratica = 0 o 5 = Sospesa diventa stato Sospesa (= 4)&#xA;&#xA;&#xA;UPDATE [TB_AULDR_RAPPORTI_LD] &#xA;SET auldr_stato_posizione = 1 &#xA;WHERE ([AULDR_STATO_LAVORAZIONE] = 0) and auldr_stato_pratica = 1&#xA;&#xA;UPDATE [TB_AULDR_RAPPORTI_LD] &#xA;SET auldr_stato_posizione = 7 &#xA;WHERE ([AULDR_STATO_LAVORAZIONE] = 0) and auldr_stato_pratica = 9&#xA;&#xA;UPDATE [TB_AULDR_RAPPORTI_LD] &#xA;SET auldr_stato_posizione = 8&#xA;WHERE ([AULDR_STATO_LAVORAZIONE] = 0) and auldr_stato_pratica = 2&#xA;&#xA;UPDATE [TB_AULDR_RAPPORTI_LD] &#xA;SET auldr_stato_posizione =  4&#xA;WHERE ([AULDR_STATO_LAVORAZIONE] = 0) and auldr_stato_pratica IN (0,5)&#xA;&#xA;&#xA;-- 2 -- Commentato il 27 febbraio 2018&#xA;-- Poniamo la Posizione in stato ANNULLATA (7)&#xA;--UPDATE [TB_AULDR_RAPPORTI_LD]&#xA;--SET auldr_stato_posizione = 7&#xA;--WHERE auldr_data_cessazione is not null &#xA;--AND ([AULDR_STATO_LAVORAZIONE] = 0)&#xA;--AND ([AULDR_STATO_PRATICA] IN (9,2));&#xA;&#xA;---&#xA;--- Valorizzazione codice sede Inps&#xA;---&#xA; UPDATE TB_AULDR_RAPPORTI_LD&#xA; SET    auldr_ausin = tb_ausin_sedi_inps_ct.ausin_codice_pk&#xA; FROM   TB_AULDR_RAPPORTI_LD INNER JOIN tb_ausin_sedi_inps_ct ON &#xA; SUBSTRING(TB_AULDR_RAPPORTI_LD.auldr_sede_inps,1,4) = tb_ausin_sedi_inps_ct.ausin_codice_sede&#xA; WHERE  ([AULDR_STATO_LAVORAZIONE] = 0);&#xA;&#xA; UPDATE TB_AULDR_RAPPORTI_LD&#xA; SET auldr_ausin = NULL&#xA; WHERE auldr_ausin = 0;&#xA;&#xA;--- Inserito il 27 febbraio 2018&#xA;--- Valorizzazione denominazione con default nel caso di denominazione vuota&#xA;---&#xA;update [TB_AULDR_RAPPORTI_LD]&#xA;set auldr_denominazione = 'X' &#xA;where  auldr_denominazione is null or rtrim(ltrim(auldr_denominazione))= ''&#xA;&#xA;--- Modificato il 27 febbraio 2018: seleziona il valore di aupoc_data_ultimo_stato da auldr_data_decorrenza_stato&#xA;--- Inserimento su AUPOC&#xA;---&#xA;INSERT INTO [dbo].[tb_aupoc_pos_contr] (&#xA;&#x9; [aupoc_ausca_codice_pk]&#xA;&#x9;,[aupoc_auapp_codice_pk]&#xA;&#x9;,[aupoc_auspc_codice_pk]&#xA;&#x9;,[aupoc_denom_posiz_contr]&#xA;&#x9;,[aupoc_posizione]&#xA;&#x9;,[aupoc_data_inizio_attivita]&#xA;&#x9;,[aupoc_note]&#xA;&#x9;,[aupoc_data_domanda_iscr]&#xA;&#x9;,[aupoc_data_modifica]&#xA;&#x9;,[aupoc_descr_utente]&#xA;&#x9;,[aupoc_ausin_codice_pk]&#xA;&#x9;,[aupoc_aurea_codice_pk]&#xA;&#x9;,[aupoc_chiave_LD]&#xA;&#x9;,[aupoc_cfLavoratore_LD]&#xA;&#x9;,aupoc_codice_stato_LD&#xA;&#x9;,aupoc_descr_stato_LD&#xA;&#x9;,aupoc_data_cessazione_LD&#xA;&#x9;,aupoc_data_ultimo_stato )&#xA; ( &#xA; SELECT&#xA;    [AULDR_CODICE_AUSCA]&#xA;   ,6 as AUAPP&#xA;   ,auldr_stato_posizione&#xA;   ,auldr_denominazione as denominazione&#xA;   ,[auldr_progressivo_rappORto] as posizione&#xA;   ,[auldr_data_assunzione]&#xA;   ,'Proveniente da Lavoratori Domestici' as Note&#xA;   ,[auldr_data_presentazione]&#xA;   ,getdate()&#xA;   ,'Lavoratori Domestici' as DescrUtente&#xA;   ,auldr_ausin as SedeInps&#xA;   ,11 as AUREA&#xA;   ,[auldr_chiave_gestionale_DatORe]&#xA;   ,upper([auldr_codice_fiscale_lavORatORe])&#xA;   ,auldr_stato_pratica as codice_stato_pratica&#xA;   ,auldr_descr_stato_pratica as descr_stato_pratica&#xA;   ,CASE IsNUll(auldr_data_cessazione,'29991231') when '29991231' then null  else CONVERT(date,auldr_data_cessazione) END As Data_Cessazione&#xA;   ,auldr_data_decorrenza_stato&#xA;  FROM [dbo].[TB_AULDR_RAPPORTI_LD]&#xA;  WHERE [AULDR_STATO_LAVORAZIONE] = 0 &#xA;  AND [AULDR_CODICE_AUSCA] &lt;&gt; 0&#xA;  AND [AULDR_VALIDATO_AUPOC] = 0);&#xA;  &#xA;--- Modificato il 27 febbraio 2018: seleziona il valore di aupoc_data_ultimo_stato da auldr_data_decorrenza_stato&#xA;--- Modifica su AUPOC&#xA;--- &#xA;UPDATE [dbo].[tb_aupoc_pos_contr]&#xA;SET  [aupoc_denom_posiz_contr] = auldr_denominazione&#xA;    ,[aupoc_data_inizio_attivita] = [auldr_data_assunzione]&#xA;    ,[aupoc_data_domanda_iscr] = [auldr_data_presentazione]&#xA;    ,[aupoc_data_modifica] = getdate()&#xA;    ,[aupoc_descr_utente] = 'Lavoratori Domestici'&#xA;    ,[aupoc_ausin_codice_pk] = auldr_ausin&#xA;    ,[aupoc_chiave_LD] = [auldr_chiave_gestionale_datore]&#xA;    ,[aupoc_cfLavoratore_LD] = UPPER([auldr_codice_fiscale_lavoratore])&#xA;&#x9;, aupoc_codice_stato_LD = auldr_stato_pratica&#xA;&#x9;, aupoc_descr_stato_LD = auldr_descr_stato_pratica&#xA;&#x9;, aupoc_data_cessazione_LD = CASE IsNUll(auldr_data_cessazione,'29991231') &#xA;&#x9;&#x9; WHEN '29991231' THEN null ELSE CONVERT(date,auldr_data_cessazione) END&#xA;&#x9;, aupoc_data_ultimo_stato = auldr_data_decorrenza_stato&#xA;&#x9;, aupoc_auspc_codice_pk = auldr_stato_posizione&#xA;&#xA;FROM [dbo].[tb_aupoc_pos_contr] inner join [TB_AULDR_RAPPORTI_LD] ON  &#xA;&#x9; aupoc_codice_pk = [AULDR_CODICE_AUPOC]&#xA;WHERE [AULDR_VALIDATO_AUPOC] = 1 &#xA;AND [AULDR_FLAG_AUPOC] = 'S' &#xA;AND [aupoc_aurea_codice_pk] = 11 ;&#xA;&#xA;---&#xA;--- Pone AULDR_VALIDATO_AUSCA = 2 per i soli record appena inseriti: servirà per auind ed auvas&#xA;---&#xA;&#xA;UPDATE  TB_AULDR_RAPPORTI_LD&#xA;SET [AULDR_VALIDATO_AUPOC] = 2, &#xA;    [AULDR_FLAG_AUPOC] = 'S', &#xA;    [AULDR_CODICE_AUPOC] = aupoc_codice_pk&#xA;FROM TB_AULDR_RAPPORTI_LD INNER JOIN tb_aupoc_pos_contr ON aupoc_posizione = AULDR_progressivo_rapporto&#xA;WHERE [AULDR_STATO_LAVORAZIONE] = 0 &#xA;AND aupoc_aurea_codice_pk = 11 &#xA;AND [AULDR_VALIDATO_AUPOC] = 0 ;&#xA;&#xA;--- Modificato il 27 febbraio 2018: eliminata la cancellazione&#xA;--- Cancellazione logica AUVAS&#xA;---&#xA;&#xA;--UPDATE  tb_auvas_var_stato_pos&#xA;--SET auvas_rec_del = 'S', auvas_data_del = GETDATE()&#xA;--FROM TB_AULDR_RAPPORTI_LD INNER JOIN tb_auvas_var_stato_pos ON &#xA;--&#x9; TB_AULDR_RAPPORTI_LD.AULDR_CODICE_AUPOC = tb_auvas_var_stato_pos.auvas_aupoc_codice_pk INNER JOIN tb_aupoc_pos_contr ON tb_auvas_var_stato_pos.auvas_aupoc_codice_pk = tb_aupoc_pos_contr.aupoc_codice_pk&#xA;--WHERE (TB_AULDR_RAPPORTI_LD.AULDR_STATO_LAVORAZIONE = 0) &#xA;--AND ([AULDR_VALIDATO_AUPOC] &gt; 0) &#xA;--AND aupoc_aurea_codice_pk = 11 ;&#xA;&#xA;---&#xA;--- Cancellazione logica AUIND&#xA;---&#xA; UPDATE tb_auind_indirizzi&#xA; SET auind_rec_del = 'S', auind_data_del = GETDATE()&#xA; FROM TB_AULDR_RAPPORTI_LD INNER JOIN tb_auind_indirizzi ON &#xA;   TB_AULDR_RAPPORTI_LD.AULDR_CODICE_AUPOC = tb_auind_indirizzi.auind_tabella_codice_pk INNER JOIN&#xA;         tb_aupoc_pos_contr ON tb_auind_indirizzi.auind_tabella_codice_pk = tb_aupoc_pos_contr.aupoc_codice_pk&#xA; WHERE (TB_AULDR_RAPPORTI_LD.AULDR_STATO_LAVORAZIONE = 0) &#xA; AND ([AULDR_VALIDATO_AUPOC] &gt; 0) &#xA; AND (tb_auind_indirizzi.auind_tabella = 'AUPOC') &#xA; AND (tb_auind_indirizzi.auind_aurea_codice_pk = 11) &#xA; AND (tb_aupoc_pos_contr.aupoc_aurea_codice_pk = 11) ;&#xA;&#xA;--- &#xA;--- Inserimento su AUVAS&#xA;---&#xA;&#xA;--- Modificata il 27 febbraio 2018: eliminato l'inserimento dello stato Aperta&#xA;--- Inserimento variazioni di stato Aperte&#xA;---&#xA;&#xA;-- INSERT INTO [dbo].[tb_auvas_var_stato_pos] (&#xA;--   [auvas_aupoc_codice_pk]&#xA;--             ,[auvas_auspc_codice_pk]&#xA;--             ,[auvas_data_variazione_stato]&#xA;--             ,[auvas_utente]&#xA;--             ,[auvas_dispositivo_utente]&#xA;--             ,[auvas_data_modifica]&#xA;--             ,[auvas_descr_utente] )&#xA;--  (&#xA;--  SELECT [AULDR_CODICE_AUPOC]&#xA;--   ,1 as StatoAperta&#xA;--   ,[auldr_data_assunzione] as dataVariazioneStato&#xA;--   ,'' as Utente&#xA;--   ,'' as dispositivo&#xA;--   ,getdate()&#xA;--   ,'Lavoratori Domestici' as DescrUtente&#xA;--  FROM [dbo].[TB_AULDR_RAPPORTI_LD]&#xA;--  WHERE [AULDR_STATO_LAVORAZIONE] = 0 &#xA;--  AND [AULDR_CODICE_AUPOC] &lt;&gt; 0 &#xA;--  AND auldr_stato_posizione &gt; 0&#xA;--     )&#xA;&#xA;--- Modificata il 27 febbraio 2018: eliminato l'inserimento dello stato Annullate&#xA;--- Inserimento variazioni di stato Annullate/Cessate&#xA;---&#xA;-- 1 -- &#xA;-- Inserimento ANNULLATE&#xA;--  INSERT INTO [dbo].[tb_auvas_var_stato_pos] (&#xA;--    [auvas_aupoc_codice_pk]&#xA;--  ,[auvas_auspc_codice_pk]&#xA;--  ,[auvas_data_variazione_stato]&#xA;--  ,[auvas_utente]&#xA;--  ,[auvas_dispositivo_utente]&#xA;--  ,[auvas_data_modifica]&#xA;--  ,[auvas_descr_utente] )&#xA;--  (&#xA;--  &#x9;SELECT &#xA;--  &#x9;&#x9;[AULDR_CODICE_AUPOC]&#xA;--  &#x9;   , 7 as StatoAnnullata&#xA;--  &#x9;   , auldr_data_modifica as dataVariazioneStato&#xA;--  &#x9;   , '' as Utente&#xA;--  &#x9;   , '' as dispositivo&#xA;--  &#x9;   , Getdate() As DataModifica&#xA;--  &#x9;   , 'Lavoratori Domestici' as DescrUtente&#xA;--  &#x9;FROM [dbo].[TB_AULDR_RAPPORTI_LD]&#xA;--  &#x9;WHERE [AULDR_STATO_LAVORAZIONE] = 0 &#xA;--  &#x9;AND [AULDR_CODICE_AUPOC] &lt;&gt; 0 &#xA;--  &#x9;AND auldr_stato_posizione = 7&#xA;--  )&#xA;-- 2 -- &#xA;--- Modificata il 27 febbraio 2018: eliminato l'inserimento dello stato Cessate&#xA; -- Inserimento CESSATE&#xA;--  INSERT INTO [dbo].[tb_auvas_var_stato_pos] (&#xA;--    [auvas_aupoc_codice_pk]&#xA;--  ,[auvas_auspc_codice_pk]&#xA;--  ,[auvas_data_variazione_stato]&#xA;--  ,[auvas_utente]&#xA;--  ,[auvas_dispositivo_utente]&#xA;--  ,[auvas_data_modifica]&#xA;--  ,[auvas_descr_utente] )&#xA;--  (&#xA;--  &#x9;SELECT [AULDR_CODICE_AUPOC]&#xA;--  &#x9;&#x9;, 3 as StatoCessata&#xA;--  &#x9;&#x9;, auldr_data_cessazione as dataVariazioneStato&#xA;--  &#x9;&#x9;, '' as Utente&#xA;--  &#x9;&#x9;, '' as dispositivo&#xA;--  &#x9;&#x9;, Getdate() As DataModifica&#xA;--  &#x9;&#x9;, 'Lavoratori Domestici' as DescrUtente&#xA;--  &#x9;FROM [dbo].[TB_AULDR_RAPPORTI_LD]&#xA;--  &#x9;WHERE 1 = 1&#xA;--  &#x9;AND [AULDR_STATO_LAVORAZIONE] = 0 &#xA;--  &#x9;AND [AULDR_CODICE_AUPOC] &lt;&gt; 0 &#xA;--  &#x9;AND auldr_stato_posizione = 1&#xA;--  &#x9;AND convert(date,auldr_data_cessazione) &lt;= Convert(date,Getdate())&#xA;--  )&#xA;&#xA;---&#xA;---Inserito 27 febbraio 2018: inserita la valorizzazione del flag di presenza su auvas &#xA;---&#xA;Update [TB_AULDR_RAPPORTI_LD]&#xA;set auldr_flag_auvas = 1&#xA;from [TB_AULDR_RAPPORTI_LD] inner join tb_auvas_var_stato_pos on auvas_aupoc_codice_pk = [AULDR_CODICE_AUPOC] &#xA;where [auldr_stato_posizione] = [auvas_auspc_codice_pk] and [auldr_data_decorrenza_stato] = auvas_data_variazione_stato&#xA;  &#x9;AND [AULDR_STATO_LAVORAZIONE] = 0 &#xA; &#x9;AND [AULDR_CODICE_AUPOC] &lt;&gt; 0 &#xA;&#x9;AND [auldr_data_decorrenza_stato] is not null&#xA;&#xA;---&#xA;---Inserito 27 febbraio 2018: inserimento su AUVAS se non esiste &#xA;---&#xA;  INSERT INTO [dbo].[tb_auvas_var_stato_pos] (&#xA;    &#x9; [auvas_aupoc_codice_pk]&#xA;  &#x9;,[auvas_auspc_codice_pk]&#xA;  &#x9;,[auvas_data_variazione_stato]&#xA;  &#x9;,[auvas_utente]&#xA;  &#x9;,[auvas_dispositivo_utente]&#xA;  &#x9;,[auvas_data_modifica]&#xA;  &#x9;,[auvas_descr_utente] )&#xA;  (&#xA;  &#x9;SELECT [AULDR_CODICE_AUPOC]&#xA;  &#x9;&#x9;, auldr_stato_posizione &#xA;  &#x9;&#x9;, auldr_data_decorrenza_stato &#xA;  &#x9;&#x9;, '' as Utente&#xA;  &#x9;&#x9;, '' as dispositivo&#xA;  &#x9;&#x9;, Getdate() As DataModifica&#xA;  &#x9;&#x9;, 'Lavoratori Domestici' as DescrUtente&#xA;  &#x9;FROM [dbo].[TB_AULDR_RAPPORTI_LD]&#xA;  &#x9;WHERE 1 = 1&#xA;  &#x9;AND [AULDR_STATO_LAVORAZIONE] = 0 &#xA;  &#x9;AND [AULDR_CODICE_AUPOC] &lt;&gt; 0 &#xA;  &#x9;AND auldr_flag_auvas = 0&#xA;  )&#xA;&#xA;---&#xA;--- Inserimento su AUIND&#xA;---&#xA;INSERT INTO [dbo].[tb_auind_indirizzi] (&#xA;    [auind_lingua]&#xA;   ,[auind_tabella]&#xA;   ,[auind_tabella_codice_pk]&#xA;   ,[auind_codice_toponimo]&#xA;   ,[auind_toponimo]&#xA;   ,[auind_indirizzo]&#xA;   ,[auind_civico]&#xA;   ,[auind_descr_comune]&#xA;   ,[auind_sigla_provincia]&#xA;   ,[auind_cap]&#xA;   ,[auind_chiave_interna]&#xA;   ,[auind_chiave_esterna]&#xA;   ,[auind_gestore_indirizzo]&#xA;   ,[auind_codice_qualita_ind]&#xA;   ,[auind_data_modifica]&#xA;   ,[auind_descr_utente]&#xA;   ,[auind_aurea_codice_pk] )&#xA; (&#xA; SELECT 'ITALIANO' as lingua&#xA;&#x9;,'AUPOC' as tabella&#xA;&#x9;,[AULDR_CODICE_AUPOC]&#xA;&#x9;,[auldr_codice_toponimo]             &#xA;&#x9;,[auldr_toponimo]     &#xA;&#x9;,[auldr_indirizzo]&#xA;&#x9;,auldr_civico    &#xA;&#x9;,[auldr_comune]     &#xA;&#x9;,[auldr_sigla_provincia]     &#xA;&#x9;,[auldr_cap]&#xA;&#x9;,[auldr_progressivo_rapporto] as chiaveInterna&#xA;&#x9;,null as chiaveEsterna&#xA;&#x9;,'LD' as gestoreindirizzo&#xA;&#x9;,[auldr_indice_qualita_indirizzo] as codiceQualita&#xA;&#x9;,getdate() as dataModifica&#xA;&#x9;,'Lavoratori Domestici' as DescrUtente &#xA;&#x9;,11 as Aurea&#xA;  FROM [dbo].[TB_AULDR_RAPPORTI_LD]&#xA;  WHERE [AULDR_STATO_LAVORAZIONE] = 0 &#xA; AND [AULDR_CODICE_AUPOC] &lt;&gt; 0 &#xA; )&#xA;-- Fine&#xA;&#xA;--------------------------------------------------------------------------------------------&#xA;---&#xA;-- &#x9;2016 06 14 - Processo di CHIUSURA POSIZIONI SCADUTE &#xA;---&#xA;-- Crea la tabella temporanea&#xA;---&#xA;create table #temp_LD (codice_pk int null, data_inizio date null, data_fine date null) on [primary];&#xA;&#xA;---&#xA;-- Inserisce i record da lavorare nella tabella temporanea&#xA;---&#xA;INSERT INTO #temp_LD(codice_pk, data_inizio, data_fine)&#xA;&#xA;&#x9;SELECT  aupoc_codice_pk, &#xA;&#x9;&#x9;&#x9;aupoc_data_inizio_attivita, &#xA;&#x9;&#x9;&#x9;aupoc_data_cessazione_LD &#xA;&#x9;FROM tb_aupoc_pos_contr&#xA;&#x9;WHERE aupoc_aurea_codice_pk = 11 and   -- Posizione LD&#xA;&#x9;&#x9;  aupoc_auspc_codice_pk = 1  and   -- Stato posizione Attiva&#xA;&#x9;&#x9;  aupoc_codice_stato_LD = 1  and   -- Stato pratica accolto&#xA;&#x9;&#x9;  aupoc_data_cessazione_LD &lt;= convert(date,getdate())&#xA;&#xA;--- Modificata il 27 febbraio 2018: eliminata la cancellazione logica&#xA;---&#xA;--- Cancella logicamente i record su auvas&#xA;---&#xA;&#xA;--UPDATE tb_auvas_var_stato_pos&#xA;--SET &#xA;--&#x9;[auvas_rec_del] = 'S', &#xA;--&#x9;[auvas_data_del] = getdate()&#xA;--FROM tb_auvas_var_stato_pos INNER JOIN #temp_LD ON &#xA;--&#x9; auvas_aupoc_codice_pk = codice_pk&#xA;&#xA;&#xA;---&#xA;--- INSERIMENTO SU AUVAS&#xA;---&#xA;&#xA;--- Modificata il 27 febbraio 2018: eliminata l'inserimento variazione di stato Aperte&#xA;--- Inserimento variazioni di stato Aperte&#xA;---&#xA;&#xA;-- INSERT INTO [dbo].[tb_auvas_var_stato_pos] (&#xA;--   [auvas_aupoc_codice_pk]&#xA;-- ,[auvas_auspc_codice_pk]&#xA;-- ,[auvas_data_variazione_stato]&#xA;-- ,[auvas_utente]&#xA;-- ,[auvas_dispositivo_utente]&#xA;-- ,[auvas_data_modifica]&#xA;-- ,[auvas_descr_utente] )&#xA;--  (&#xA;-- &#x9;SELECT codice_pk&#xA;-- &#x9;  , 1 as StatoAperta&#xA;-- &#x9;  , data_inizio as DataVariazioneStato&#xA;-- &#x9;  , '' as Utente&#xA;-- &#x9;  , '' as Dispositivo&#xA;-- &#x9;  , Getdate() As DataModifica&#xA;-- &#x9;  , 'Lavoratori Domestici' as DescrUtente&#xA;-- &#x9;FROM #temp_LD&#xA;-- )&#xA;&#xA;---&#xA;--- Inserimento variazioni di stato Cessate &#xA;---&#xA;INSERT INTO [dbo].[tb_auvas_var_stato_pos] (&#xA;&#x9;  [auvas_aupoc_codice_pk]&#xA;&#x9;,[auvas_auspc_codice_pk]&#xA;&#x9;,[auvas_data_variazione_stato]&#xA;&#x9;,[auvas_utente]&#xA;&#x9;,[auvas_dispositivo_utente]&#xA;&#x9;,[auvas_data_modifica]&#xA;&#x9;,[auvas_descr_utente] )&#xA;(&#xA;&#x9;SELECT codice_pk&#xA;&#x9;&#x9;   , 3 as StatoCessata&#xA;&#x9;&#x9;   , data_fine as dataVariazioneStato&#xA;&#x9;&#x9;   , '' as Utente&#xA;&#x9;&#x9;   , '' as dispositivo&#xA;&#x9;&#x9;   , Getdate() As DataModifica&#xA;&#x9;&#x9;   , 'Lavoratori Domestici' as DescrUtente&#xA;&#x9;FROM #temp_LD&#xA;)&#xA;&#xA;---&#xA;--- Aggiornamento AUPOC&#xA;---&#xA;UPDATE tb_aupoc_pos_contr&#xA;SET aupoc_auspc_codice_pk = 3, -- Stato posizione Cessata Definitiva&#xA;    aupoc_data_ultimo_stato = aupoc_data_cessazione_LD&#xA;FROM tb_aupoc_pos_contr INNER JOIN #temp_LD ON &#xA;&#x9; aupoc_codice_pk = codice_pk&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
