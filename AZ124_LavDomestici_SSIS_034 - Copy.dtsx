          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="---&#xA;--- Gestione tabella AULDR&#xA;---&#xA;---&#xA;--- Crea tabella tempORanea&#xA;---&#xA;  CREATE TABLE dbo.#temp_progr (sequenza int null,   progressivo varchar(20) null) on [primary];&#xA;&#xA;---&#xA;--- Scarta i recORd con cf anomali pone STATO LAVORAZIONE = -1&#xA;---&#xA;UPDATE TB_AULDR_RAPPORTI_LD&#xA;SET [AULDR_STATO_LAVORAZIONE] = -1&#xA;WHERE [AULDR_STATO_LAVORAZIONE] = 0 &#xA;AND ([AULDR_codice_fiscale] is null OR len([AULDR_codice_fiscale]) &gt; 16 &#xA;OR (len( ltrim(rtrim([AULDR_codice_fiscale]))) &gt; 11 AND len(  ltrim(rtrim([AULDR_codice_fiscale]))) &lt; 16));&#xA;&#xA;---&#xA;--- Inserisce i recORd  nella tabella temporanea raggruppANDoli per progressivo rappORto&#xA;---&#xA;INSERT #temp_progr (sequenza, progressivo)&#xA;    SELECT MAX([auldr_sequenza_inserimento]), auldr_progressivo_rapporto &#xA; FROM TB_AULDR_RAPPORTI_LD&#xA;    WHERE [AULDr_STATO_LAVORAZIONE] = 0 &#xA;    group by auldr_progressivo_rapporto;&#xA;&#xA;---&#xA;--- Scarto dei progressivi duplicati pone STATO LAVORAZIONE = -2&#xA;---&#xA;&#xA;   UPDATE TB_AULDR_RAPPORTI_LD&#xA;   SET [AULDR_STATO_LAVORAZIONE] = -2&#xA;   FROM TB_AULDR_RAPPORTI_LD INNER JOIN #temp_progr ON &#xA;  [auldr_sequenza_inserimento] &lt;&gt; #temp_progr.[sequenza] &#xA;  AND auldr_progressivo_rappORto = #temp_progr.progressivo&#xA;   WHERE [AULDr_STATO_LAVORAZIONE] = 0 ;&#xA;&#xA;---&#xA;--- Cancellazione tabella temporanea&#xA;---&#xA;  DROP TABLE #temp_progr ;&#xA;&#xA;---&#xA;--- Aggiornamento riferimenti esterni verso AUSCA &#xA;---&#xA;   UPDATE  TB_AULDR_RAPPORTI_LD&#xA;   SET &#xA;  [AULDR_VALIDATO_AUSCA] = 1, &#xA;  [AULDR_FLAG_AUSCA] = 'S', &#xA;  [AULDR_CODICE_AUSCA] = ausca_codice_pk&#xA;   FROM TB_AULDR_RAPPORTI_LD INNER JOIN tb_ausca_sog_contr_az ON &#xA;   ausca_codice_fiscale = AULDR_codice_fiscale&#xA;   WHERE [AULDR_STATO_LAVORAZIONE] = 0 ;&#xA;&#xA;---&#xA;--- Aggiornamento riferimenti esterni verso AUPOC &#xA;---&#xA; UPDATE  TB_AULDR_RAPPORTI_LD&#xA; SET &#xA;  [AULDR_VALIDATO_AUPOC] = 1, &#xA;  [AULDR_FLAG_AUPOC] = 'S', &#xA;  [AULDR_CODICE_AUPOC] = aupoc_codice_pk&#xA;  FROM TB_AULDR_RAPPORTI_LD INNER JOIN tb_aupoc_pos_contr ON &#xA;       aupoc_posizione = AULDR_progressivo_rapporto&#xA; WHERE [AULDR_STATO_LAVORAZIONE] = 0 &#xA; AND aupoc_aurea_codice_pk = 11 ;&#xA;&#xA;---&#xA;--- Aggiornamento denominazione per persone fisiche&#xA;---&#xA;&#xA; UPDATE TB_AULDR_RAPPORTI_LD&#xA; SET auldr_denominazione = substring(auldf_cognome,1,len(TB_AULDF_PERSONA_FISICA_LD.auldf_cognome)) + ' ' + substring(auldf_nome,1,len(TB_AULDF_PERSONA_FISICA_LD.auldf_nome))&#xA; FROM TB_AULDR_RAPPORTI_LD INNER JOIN TB_AULDF_PERSONA_FISICA_LD ON &#xA;   TB_AULDR_RAPPORTI_LD.auldr_codice_fiscale = TB_AULDF_PERSONA_FISICA_LD.auldf_codice_fiscale&#xA; WHERE (TB_AULDR_RAPPORTI_LD.[AULDR_STATO_LAVORAZIONE] = 0) &#xA; AND (TB_AULDF_PERSONA_FISICA_LD.AULDF_STATO_LAVORAZIONE = 0) ;&#xA;&#xA;---&#xA;--- Aggiornamento denominazione per persone giuridiche &#xA;---&#xA;&#xA;UPDATE TB_AULDR_RAPPORTI_LD&#xA;SET auldr_denominazione = TB_AULDG_PERSONA_GIURIDICA_LD.auldg_denominazione&#xA;FROM TB_AULDR_RAPPORTI_LD INNER JOIN TB_AULDG_PERSONA_GIURIDICA_LD ON &#xA; TB_AULDR_RAPPORTI_LD.auldr_codice_fiscale = TB_AULDG_PERSONA_GIURIDICA_LD.auldg_codice_fiscale&#xA;WHERE (TB_AULDR_RAPPORTI_LD.AULDR_STATO_LAVORAZIONE = 0) &#xA;AND (TB_AULDG_PERSONA_GIURIDICA_LD.AULDg_STATO_LAVORAZIONE = 0) ;&#xA;&#xA;&#xA;-- Inizio stored&#xA;&#xA;---&#xA;--- Procedura di aggiORnamento indirizzi e toponimi&#xA;---&#xA;&#xA; --DECLARE CURSORE_INDIRIZZI CursOR&#xA; --FOR&#xA; --SELECT  [AULDR_indirizzo_input], AULDR_codice_pk&#xA; -- FROM dbo.TB_AULDR_RAPPORTI_LD&#xA; -- WHERE  ([AULDR_STATO_LAVORAZIONE] = 0) &#xA; -- AND [AULDR_indirizzo_input] is not null&#xA;       &#xA; --     OPEN CURSORE_INDIRIZZI &#xA; --    DECLARE @Indirizzo Varchar(255) &#xA; -- DECLARE @codice_pk int   &#xA; -- DECLARE @Nuovo_Indirizzo Varchar(255)    &#xA; -- DECLARE @Toponimo VARCHAR(50)&#xA; -- DECLARE @Codice_toponimo VARCHAR(3)&#xA; -- DECLARE @Ret_val INT&#xA;&#xA; --    FETCH NEXT FROM CURSORE_INDIRIZZI INTO @Indirizzo, @Codice_pk&#xA; --WHILE (@@FETCH_STATUS = 0)  &#xA; --BEGIN&#xA; -- SELECT @Nuovo_Indirizzo=Indirizzo, @Toponimo=Toponimo, @Codice_toponimo = Codice_toponimo, @Ret_val = Ret_val FROM  dbo.fn_indirizzo_e_toponimi(@Indirizzo)        &#xA; -- if @ret_val &lt;&gt; 0&#xA; -- BEGIN&#xA; --  UPDATE dbo.TB_AULDR_RAPPORTI_LD&#xA; --  SET [AULDR_indirizzo] = @nuovo_indirizzo,&#xA; --   [AULDR_toponimo]  = @toponimo,&#xA; --   [AULDR_codice_toponimo] = @codice_toponimo&#xA; --  WHERE AULDR_codice_pk = @Codice_pk&#xA; -- end &#xA; -- FETCH NEXT FROM CURSORE_INDIRIZZI INTO @Indirizzo, @Codice_pk&#xA; --END&#xA; --CLOSE CURSORE_INDIRIZZI&#xA; --DEALLOCATE CURSORE_INDIRIZZI&#xA;&#xA;---&#xA;--- Aggiorna gli indirizzi senza toponimo&#xA;---&#xA; UPDATE dbo.TB_AULDR_RAPPORTI_LD&#xA; SET [AULDR_indirizzo] = [AULDR_indirizzo_input]&#xA; WHERE        ([AULDR_STATO_LAVORAZIONE] = 0) &#xA; AND [AULDR_indirizzo_input] is not null &#xA; AND AULDR_indirizzo is null ;&#xA;&#xA;---&#xA;--- Valorizza civico &#xA;---&#xA;&#xA; --DECLARE CURSORE_INDIRIZZI Cursor&#xA; --FOR&#xA; --SELECT  [AULDR_indirizzo], AULDR_codice_pk&#xA; -- FROM dbo.TB_AULDR_RAPPORTI_LD&#xA; -- WHERE ([AULDR_STATO_LAVORAZIONE] = 0) &#xA; -- AND [AULDR_indirizzo] is not null AND rtrim(ltrim(auldr_indirizzo)) &lt;&gt; ''&#xA;       &#xA; --     OPEN CURSORE_INDIRIZZI &#xA; --    DECLARE @Indirizzo1 Varchar(255) &#xA; -- DECLARE @codice_pk1 int   &#xA; --    DECLARE @civico1 Varchar(50)    &#xA; -- DECLARE @posizione1 int&#xA; -- DECLARE @Ret_val1 INT&#xA;&#xA; --    FETCH NEXT FROM CURSORE_INDIRIZZI INTO @Indirizzo1, @Codice_pk1&#xA; --WHILE (@@FETCH_STATUS = 0)  &#xA; --BEGIN&#xA; -- SELECT @civico1=civico, @posizione1=pos_min, @Ret_val1 = Ret_val FROM  dbo.[fn_tORna_civico](@Indirizzo1)        &#xA; -- if @ret_val1 = 0&#xA; -- BEGIN&#xA; --  UPDATE dbo.TB_AULDR_RAPPORTI_LD&#xA; --   SET [AULDR_civico] = @civico1,&#xA; --   [AULDR_indirizzo]  = substring([AULDR_indirizzo],1, @posizione1-1)&#xA; --  WHERE AULDR_codice_pk = @Codice_pk1 AND @Ret_val1 = 0&#xA; -- end &#xA; -- FETCH NEXT FROM CURSORE_INDIRIZZI INTO @Indirizzo1, @Codice_pk1&#xA; --END&#xA; --CLOSE CURSORE_INDIRIZZI&#xA; --DEALLOCATE CURSORE_INDIRIZZI&#xA;&#xA;---&#xA;--- Tappi per campi indirizzo&#xA;---&#xA;&#xA; UPDATE TB_AULDR_RAPPORTI_LD&#xA; SET AULDR_toponimo = '--'&#xA; WHERE (AULDR_toponimo is null OR rtrim(ltrim(AULDR_toponimo)) = '') AND ([AULDR_STATO_LAVORAZIONE] = 0);&#xA;&#xA; UPDATE TB_AULDR_RAPPORTI_LD&#xA; SET AULDR_codice_toponimo = '--'&#xA; WHERE (AULDR_codice_toponimo is null OR rtrim(ltrim(AULDR_codice_toponimo)) = '') AND ([AULDR_STATO_LAVORAZIONE] = 0);&#xA;&#xA; UPDATE TB_AULDR_RAPPORTI_LD&#xA; SET AULDR_indirizzo = '--'&#xA; WHERE (AULDR_indirizzo is null OR rtrim(ltrim(AULDR_indirizzo)) = '') AND ([AULDR_STATO_LAVORAZIONE] = 0);&#xA;&#xA; UPDATE TB_AULDR_RAPPORTI_LD&#xA; SET AULDR_civico = 'SNC'&#xA; WHERE (AULDR_civico is null OR rtrim(ltrim(AULDR_civico)) = '') AND ([AULDR_STATO_LAVORAZIONE] = 0);&#xA;&#xA;---&#xA;--- Elimina dall'indirizzo la virgola o il punto finale e la N finale precedeuta da spazio &#xA;---&#xA;&#xA; --UPDATE TB_AULDR_RAPPORTI_LD&#xA; --SET AULDR_indirizzo = substring(AULDR_indirizzo, 1, len(AULDR_indirizzo)-1)&#xA; --WHERE (substring(AULDR_indirizzo,len(AULDR_indirizzo),1) = ',' OR substring(AULDR_indirizzo,len(AULDR_indirizzo),1) = '.') AND ([AULDR_STATO_LAVORAZIONE] = 0)&#xA; --AND len(AULDR_indirizzo) &gt; 1 ;&#xA;&#xA; --UPDATE TB_AULDR_RAPPORTI_LD&#xA; --SET AULDR_indirizzo = substring(AULDR_indirizzo, 1, len(AULDR_indirizzo)-2)&#xA; --WHERE  (substring(AULDR_indirizzo,len(AULDR_indirizzo),1) = 'N' AND substring(AULDR_indirizzo,len(AULDR_indirizzo)-1,1) = ' ') AND ([AULDR_STATO_LAVORAZIONE] = 0)&#xA; --AND len(AULDR_indirizzo) &gt; 2 ;&#xA;&#xA;---&#xA;--- Query di aggiORnamento indice di qualità indirizzo da utilizzare per AUIND&#xA;---&#xA;&#xA; UPDATE dbo.TB_AULDR_RAPPORTI_LD SET auldr_indice_qualita_indirizzo = 0&#xA; WHERE ([AULDR_STATO_LAVORAZIONE] = 0) ;&#xA;&#xA;-- 1.1 : INDICE di QUALITA = 1  - INDIRIZZO + COMUNE + PROVINCIA &#xA; --UPDATE dbo.TB_AULDR_RAPPORTI_LD&#xA; --SET auldr_indice_qualita_indirizzo = 1&#xA; --WHERE ([AULDR_STATO_LAVORAZIONE] = 0) AND&#xA; --( ISNULL(AULDR_indirizzo, '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDR_indirizzo)) &lt;&gt; '' AND  LTRIM(RTRIM(AULDR_indirizzo)) &lt;&gt; '-' AND  LTRIM(RTRIM(AULDR_indirizzo)) &lt;&gt; '--') &#xA; --AND ( ISNULL(AULDR_comune,  '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDR_comune )) &lt;&gt; '' AND  LTRIM(RTRIM(AULDR_comune))  &lt;&gt; '-' AND  LTRIM(RTRIM(AULDR_comune))  &lt;&gt; '--') &#xA; --AND ( ISNULL(AULDR_sigla_provincia, '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDR_sigla_provincia)) &lt;&gt; '' AND  LTRIM(RTRIM(AULDR_sigla_provincia)) &lt;&gt; '-' AND  LTRIM(RTRIM(AULDR_sigla_provincia)) &lt;&gt; '--');&#xA;&#xA;-- 1.2 : INDICE di QUALITA = 2  - INDIRIZZO + COMUNE + PROVINCIA + CIVICO + TOPONIMO&#xA; --UPDATE dbo.TB_AULDR_RAPPORTI_LD&#xA; --SET auldr_indice_qualita_indirizzo = 2&#xA; --WHERE ([AULDR_STATO_LAVORAZIONE] = 0) AND&#xA; --( ISNULL(AULDR_indirizzo, '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDR_indirizzo)) &lt;&gt; '' AND  LTRIM(RTRIM(AULDR_indirizzo)) &lt;&gt; '-' AND  LTRIM(RTRIM(AULDR_indirizzo)) &lt;&gt; '--') &#xA; --AND ( ISNULL(AULDR_comune,  '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDR_comune )) &lt;&gt; '' AND  LTRIM(RTRIM(AULDR_comune))  &lt;&gt; '-' AND  LTRIM(RTRIM(AULDR_comune))  &lt;&gt; '--') &#xA; --AND ( ISNULL(AULDR_sigla_provincia, '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDR_sigla_provincia)) &lt;&gt; '' AND  LTRIM(RTRIM(AULDR_sigla_provincia)) &lt;&gt; '-' AND  LTRIM(RTRIM(AULDR_sigla_provincia)) &lt;&gt; '--')&#xA; --AND ( ISNULL(AULDR_civico,   '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDR_civico)) &lt;&gt; '' AND  LTRIM(RTRIM(AULDR_civico)) &lt;&gt; '-' AND  LTRIM(RTRIM(AULDR_civico))    &lt;&gt; '--')&#xA; --AND ( ISNULL(AULDR_codice_toponimo, '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDR_codice_toponimo))   &lt;&gt; '' AND  LTRIM(RTRIM(AULDR_codice_toponimo))  &lt;&gt; '-' AND  LTRIM(RTRIM(AULDR_codice_toponimo))  &lt;&gt; '--');&#xA;&#xA;-- 1.3 : INDICE di QUALITA = 3  - INDIRIZZO + COMUNE + PROVINCIA + CIVICO + TOPONIMO + CAP&#xA; --UPDATE dbo.TB_AULDR_RAPPORTI_LD&#xA; --SET auldr_indice_qualita_indirizzo = 3&#xA; --WHERE ([AULDR_STATO_LAVORAZIONE] = 0) AND&#xA; --( ISNULL(AULDR_indirizzo, '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDR_indirizzo)) &lt;&gt; '' AND  LTRIM(RTRIM(AULDR_indirizzo)) &lt;&gt; '-' AND  LTRIM(RTRIM(AULDR_indirizzo)) &lt;&gt; '--') &#xA; --AND ( ISNULL(AULDR_comune,  '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDR_comune )) &lt;&gt; '' AND  LTRIM(RTRIM(AULDR_comune))  &lt;&gt; '-' AND  LTRIM(RTRIM(AULDR_comune))  &lt;&gt; '--') &#xA; --AND ( ISNULL(AULDR_sigla_provincia, '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDR_sigla_provincia)) &lt;&gt; '' AND  LTRIM(RTRIM(AULDR_sigla_provincia)) &lt;&gt; '-' AND  LTRIM(RTRIM(AULDR_sigla_provincia)) &lt;&gt; '--')&#xA; --AND ( ISNULL(AULDR_civico,   '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDR_civico)) &lt;&gt; '' AND  LTRIM(RTRIM(AULDR_civico)) &lt;&gt; '-' AND  LTRIM(RTRIM(AULDR_civico))    &lt;&gt; '--')&#xA; --AND ( ISNULL(AULDR_codice_toponimo, '---') &lt;&gt; '---'  AND  LTRIM(RTRIM(AULDR_codice_toponimo))   &lt;&gt; '' AND  LTRIM(RTRIM(AULDR_codice_toponimo))  &lt;&gt; '-' AND  LTRIM(RTRIM(AULDR_codice_toponimo))  &lt;&gt; '--')&#xA; --AND ( ISNULL(AULDR_cap,'---') &lt;&gt; '---'  AND LTRIM(RTRIM(AULDR_cap)) &lt;&gt; '' AND  LTRIM(RTRIM(AULDR_cap)) &lt;&gt; '' AND LTRIM(RTRIM(AULDR_cap)) &lt;&gt; '00000' ) ;&#xA;&#xA;&#xA;-- Fine stored&#xA;&#xA;---&#xA;--- Valorizzazione stato aupoc&#xA;---&#xA;   --UPDATE [TB_AULDR_RAPPORTI_LD]&#xA; --SET auldr_stato_posizione = 1&#xA;   --WHERE (auldr_data_cessazione is null OR (auldr_data_cessazione &gt; getdate())) AND ([AULDR_STATO_LAVORAZIONE] = 0)&#xA;&#xA; --UPDATE [TB_AULDR_RAPPORTI_LD]&#xA;   --SET auldr_stato_posizione = 3&#xA;   --WHERE (auldr_data_cessazione is not null AND (auldr_data_cessazione &lt;= getdate())) AND ([AULDR_STATO_LAVORAZIONE] = 0)&#xA;&#xA;   UPDATE [TB_AULDR_RAPPORTI_LD]&#xA; SET auldr_stato_posizione = 1&#xA;   WHERE ([AULDR_STATO_LAVORAZIONE] = 0);&#xA;&#xA; UPDATE [TB_AULDR_RAPPORTI_LD]&#xA;   SET auldr_stato_posizione = 3&#xA;   WHERE auldr_data_cessazione is not null AND ([AULDR_STATO_LAVORAZIONE] = 0);&#xA;&#xA;---&#xA;--- Valorizzazione codice sede Inps&#xA;---&#xA; UPDATE TB_AULDR_RAPPORTI_LD&#xA; SET    auldr_ausin = tb_ausin_sedi_inps_ct.ausin_codice_pk&#xA; FROM   TB_AULDR_RAPPORTI_LD INNER JOIN&#xA;               tb_ausin_sedi_inps_ct ON substring(TB_AULDR_RAPPORTI_LD.auldr_sede_inps,1,4) = tb_ausin_sedi_inps_ct.ausin_codice_sede&#xA;   WHERE  ([AULDR_STATO_LAVORAZIONE] = 0);&#xA;&#xA; UPDATE TB_AULDR_RAPPORTI_LD&#xA; SET auldr_ausin = NULL&#xA; WHERE auldr_ausin = 0;&#xA;---&#xA;--- Inserimento su AUPOC&#xA;---&#xA;-- Sto qui&#xA; INSERT INTO [dbo].[tb_aupoc_pos_contr] (&#xA;&#x9; [aupoc_ausca_codice_pk]&#xA;&#x9;,[aupoc_auapp_codice_pk]&#xA;&#x9;,[aupoc_auspc_codice_pk]&#xA;&#x9;,[aupoc_denom_posiz_contr]&#xA;&#x9;,[aupoc_posizione]&#xA;&#x9;,[aupoc_data_inizio_attivita]&#xA;&#x9;,[aupoc_note]&#xA;&#x9;,[aupoc_data_domanda_iscr]&#xA;&#x9;,[aupoc_data_modifica]&#xA;&#x9;,[aupoc_descr_utente]&#xA;&#x9;,[aupoc_ausin_codice_pk]&#xA;&#x9;,[aupoc_aurea_codice_pk]&#xA;&#x9;,[aupoc_chiave_LD]&#xA;&#x9;,[aupoc_cfLavoratore_LD]&#xA;&#x9;,aupoc_codice_stato_LD&#xA;&#x9;,aupoc_descr_stato_LD&#xA;&#x9;,aupoc_data_cessazione_LD&#xA;&#x9;,aupoc_data_ultimo_stato )&#xA; ( &#xA; SELECT&#xA;    [AULDR_CODICE_AUSCA]&#xA;   ,6 as AUAPP&#xA;   ,auldr_stato_posizione&#xA;   ,auldr_denominazione as denominazione&#xA;   ,[auldr_progressivo_rappORto] as posizione&#xA;   ,[auldr_data_assunzione]&#xA;   ,'Proveniente da Lavoratori Domestici' as Note&#xA;   ,[auldr_data_presentazione]&#xA;   ,getdate()&#xA;   ,'Lavoratori Domestici' as DescrUtente&#xA;   ,auldr_ausin as SedeInps&#xA;   ,11 as AUREA&#xA;   ,[auldr_chiave_gestionale_DatORe]&#xA;   ,upper([auldr_codice_fiscale_lavORatORe])&#xA;   ,auldr_stato_pratica as codice_stato_pratica&#xA;   ,auldr_descr_stato_pratica as descr_stato_pratica&#xA;   ,CASE IsNUll(auldr_data_cessazione,'29991231') when '29991231' then null  else CONVERT(date,auldr_data_cessazione) END As Data_Cessazione&#xA;   ,case auldr_stato_posizione when 1 then null  when 3 then auldr_data_cessazione  end as dataUltimoStato&#xA;  FROM [dbo].[TB_AULDR_RAPPORTI_LD]&#xA;  WHERE [AULDR_STATO_LAVORAZIONE] = 0 &#xA;  AND [AULDR_CODICE_AUSCA] &lt;&gt; 0&#xA;  AND [AULDR_VALIDATO_AUPOC] = 0);&#xA;  &#xA;--- Modifica su AUPOC&#xA;UPDATE [dbo].[tb_aupoc_pos_contr]&#xA;SET  [aupoc_denom_posiz_contr] = auldr_denominazione&#xA;    ,[aupoc_data_inizio_attivita] = [auldr_data_assunzione]&#xA;    ,[aupoc_data_domanda_iscr] = [auldr_data_presentazione]&#xA;    ,[aupoc_data_modifica] = getdate()&#xA;    ,[aupoc_descr_utente] = 'Lavoratori Domestici'&#xA;    ,[aupoc_ausin_codice_pk] = auldr_ausin&#xA;    ,[aupoc_chiave_LD] = [auldr_chiave_gestionale_DatORe]&#xA;    ,[aupoc_cfLavoratore_LD] = UPPER([auldr_codice_fiscale_lavORatORe])&#xA;&#x9;, aupoc_codice_stato_LD = auldr_stato_pratica&#xA;&#x9;, aupoc_descr_stato_LD = auldr_descr_stato_pratica&#xA;&#x9;, aupoc_data_cessazione_LD = CASE IsNUll(auldr_data_cessazione,'29991231') &#xA;&#x9;&#x9; WHEN '29991231' THEN null ELSE CONVERT(date,auldr_data_cessazione) END&#xA;&#x9;, aupoc_data_ultimo_stato = CASE auldr_stato_posizione&#xA;         WHEN 1 THEN null WHEN 3 THEN auldr_data_cessazione END&#xA;FROM [dbo].[tb_aupoc_pos_contr] inner join [TB_AULDR_RAPPORTI_LD] ON  &#xA;&#x9; aupoc_codice_pk = [AULDR_CODICE_AUPOC]&#xA;WHERE [AULDR_VALIDATO_AUPOC] = 1 &#xA;AND [AULDR_FLAG_AUPOC] = 'S' &#xA;AND [aupoc_aurea_codice_pk] = 11 ;&#xA;&#xA;---&#xA;--- Pone AULDR_VALIDATO_AUSCA = 2 per i soli record appena inseriti: servirà per auind ed auvas&#xA;---&#xA;&#xA;UPDATE  TB_AULDR_RAPPORTI_LD&#xA;SET [AULDR_VALIDATO_AUPOC] = 2, &#xA;    [AULDR_FLAG_AUPOC] = 'S', &#xA;    [AULDR_CODICE_AUPOC] = aupoc_codice_pk&#xA;FROM TB_AULDR_RAPPORTI_LD INNER JOIN tb_aupoc_pos_contr ON aupoc_posizione = AULDR_progressivo_rapporto&#xA;WHERE [AULDR_STATO_LAVORAZIONE] = 0 &#xA;AND aupoc_aurea_codice_pk = 11 &#xA;AND [AULDR_VALIDATO_AUPOC] = 0 ;&#xA;&#xA;---&#xA;--- Cancellazione logica AUVAS&#xA;---&#xA;&#xA;UPDATE  tb_auvas_var_stato_pos&#xA;SET auvas_rec_del = 'S', auvas_data_del = GETDATE()&#xA;FROM TB_AULDR_RAPPORTI_LD INNER JOIN tb_auvas_var_stato_pos ON &#xA;&#x9; TB_AULDR_RAPPORTI_LD.AULDR_CODICE_AUPOC = tb_auvas_var_stato_pos.auvas_aupoc_codice_pk INNER JOIN tb_aupoc_pos_contr ON tb_auvas_var_stato_pos.auvas_aupoc_codice_pk = tb_aupoc_pos_contr.aupoc_codice_pk&#xA;WHERE (TB_AULDR_RAPPORTI_LD.AULDR_STATO_LAVORAZIONE = 0) &#xA;AND ([AULDR_VALIDATO_AUPOC] &gt; 0) &#xA;AND aupoc_aurea_codice_pk = 11 ;&#xA;&#xA;---&#xA;--- Cancellazione logica AUIND&#xA;---&#xA; UPDATE tb_auind_indirizzi&#xA; SET auind_rec_del = 'S', auind_data_del = GETDATE()&#xA; FROM TB_AULDR_RAPPORTI_LD INNER JOIN tb_auind_indirizzi ON &#xA;   TB_AULDR_RAPPORTI_LD.AULDR_CODICE_AUPOC = tb_auind_indirizzi.auind_tabella_codice_pk INNER JOIN&#xA;         tb_aupoc_pos_contr ON tb_auind_indirizzi.auind_tabella_codice_pk = tb_aupoc_pos_contr.aupoc_codice_pk&#xA; WHERE (TB_AULDR_RAPPORTI_LD.AULDR_STATO_LAVORAZIONE = 0) &#xA; AND ([AULDR_VALIDATO_AUPOC] &gt; 0) &#xA; AND (tb_auind_indirizzi.auind_tabella = 'AUPOC') &#xA; AND (tb_auind_indirizzi.auind_aurea_codice_pk = 11) &#xA; AND (tb_aupoc_pos_contr.aupoc_aurea_codice_pk = 11) ;&#xA;&#xA;---&#xA;--- Inserimento su AUVAS&#xA;---&#xA;&#xA;---&#xA;--- Inserimento variazioni di stato Aperte&#xA;---&#xA;&#xA; INSERT INTO [dbo].[tb_auvas_var_stato_pos] (&#xA;  [auvas_aupoc_codice_pk]&#xA;            ,[auvas_auspc_codice_pk]&#xA;            ,[auvas_data_variazione_stato]&#xA;            ,[auvas_utente]&#xA;            ,[auvas_dispositivo_utente]&#xA;            ,[auvas_data_modifica]&#xA;            ,[auvas_descr_utente] )&#xA; (&#xA; SELECT [AULDR_CODICE_AUPOC]&#xA;  ,1 as StatoAperta&#xA;  ,[auldr_data_assunzione] as dataVariazioneStato&#xA;  ,'-' as Utente&#xA;  ,'-' as dispositivo&#xA;  ,getdate()&#xA;  ,'Lavoratori Domestici' as DescrUtente&#xA; FROM [dbo].[TB_AULDR_RAPPORTI_LD]&#xA; WHERE [AULDR_STATO_LAVORAZIONE] = 0 &#xA; AND [AULDR_CODICE_AUPOC] &lt;&gt; 0 &#xA; AND auldr_stato_posizione &gt; 0&#xA;    )&#xA;&#xA;---&#xA;--- Inserimento variazioni di stato Cessate&#xA;---&#xA;&#xA; INSERT INTO [dbo].[tb_auvas_var_stato_pos] (&#xA;  [auvas_aupoc_codice_pk]&#xA;            ,[auvas_auspc_codice_pk]&#xA;            ,[auvas_data_variazione_stato]&#xA;            ,[auvas_utente]&#xA;            ,[auvas_dispositivo_utente]&#xA;            ,[auvas_data_modifica]&#xA;            ,[auvas_descr_utente] )&#xA; (&#xA; SELECT [AULDR_CODICE_AUPOC]&#xA;&#x9;,3 as StatoCessata&#xA;&#x9;,auldr_data_cessazione as dataVariazioneStato&#xA;&#x9;,'' as Utente&#xA;&#x9;,'' as dispositivo&#xA;&#x9;,getdate()&#xA;&#x9;,'Lavoratori Domestici' as DescrUtente&#xA; FROM [dbo].[TB_AULDR_RAPPORTI_LD]&#xA; WHERE [AULDR_STATO_LAVORAZIONE] = 0 &#xA; AND [AULDR_CODICE_AUPOC] &lt;&gt; 0 AND auldr_stato_posizione = 3&#xA; )&#xA;&#xA;---&#xA;--- Inserimento su AUIND&#xA;---&#xA;INSERT INTO [dbo].[tb_auind_indirizzi] (&#xA;    [auind_lingua]&#xA;   ,[auind_tabella]&#xA;   ,[auind_tabella_codice_pk]&#xA;   ,[auind_codice_toponimo]&#xA;   ,[auind_toponimo]&#xA;   ,[auind_indirizzo]&#xA;   ,[auind_civico]&#xA;   ,[auind_descr_comune]&#xA;   ,[auind_sigla_provincia]&#xA;   ,[auind_cap]&#xA;   ,[auind_chiave_interna]&#xA;   ,[auind_chiave_esterna]&#xA;   ,[auind_gestore_indirizzo]&#xA;   ,[auind_codice_qualita_ind]&#xA;   ,[auind_data_modifica]&#xA;   ,[auind_descr_utente]&#xA;   ,[auind_aurea_codice_pk] )&#xA; (&#xA; SELECT 'ITALIANO' as lingua&#xA;&#x9;,'AUPOC' as tabella&#xA;&#x9;,[AULDR_CODICE_AUPOC]&#xA;&#x9;,[auldr_codice_toponimo]             &#xA;&#x9;,[auldr_toponimo]     &#xA;&#x9;,[auldr_indirizzo]&#xA;&#x9;,auldr_civico    &#xA;&#x9;,[auldr_comune]     &#xA;&#x9;,[auldr_sigla_provincia]     &#xA;&#x9;,[auldr_cap]&#xA;&#x9;,[auldr_progressivo_rapporto] as chiaveInterna&#xA;&#x9;,null as chiaveEsterna&#xA;&#x9;,'LD' as GestOReIndirizzo&#xA;&#x9;,[auldr_indice_qualita_indirizzo] as codiceQualita&#xA;&#x9;,getdate() as dataModifica&#xA;&#x9;,'Lavoratori Domestici' as DescrUtente &#xA;&#x9;,11 as Aurea&#xA;  FROM [dbo].[TB_AULDR_RAPPORTI_LD]&#xA;  WHERE [AULDR_STATO_LAVORAZIONE] = 0 &#xA; AND [AULDR_CODICE_AUPOC] &lt;&gt; 0 &#xA; )&#xA;&#xA; &#xA; -- 18/12/2015 (Raffaele)&#xA; --- Modifica su AUPOC&#xA;&#xA;UPDATE [dbo].[tb_aupoc_pos_contr]&#xA;SET  [aupoc_denom_posiz_contr] = auldr_denominazione&#xA;    ,[aupoc_data_inizio_attivita] = [auldr_data_assunzione]&#xA;    ,[aupoc_data_domanda_iscr] = [auldr_data_presentazione]&#xA;    ,[aupoc_data_modifica] = getdate()&#xA;    ,[aupoc_descr_utente] = 'Lavoratori Domestici'&#xA;    ,[aupoc_ausin_codice_pk] = auldr_ausin&#xA;    ,[aupoc_chiave_LD] = [auldr_chiave_gestionale_DatORe]&#xA;    ,[aupoc_cfLavoratore_LD] = UPPER([auldr_codice_fiscale_lavORatORe])&#xA;&#x9;, aupoc_codice_stato_LD = auldr_stato_pratica&#xA;&#x9;, aupoc_descr_stato_LD = auldr_descr_stato_pratica&#xA;&#x9;, aupoc_data_cessazione_LD = CASE IsNUll(auldr_data_cessazione,'29991231') &#xA;&#x9;&#x9; WHEN '29991231' THEN null ELSE CONVERT(date,auldr_data_cessazione) END&#xA;&#x9;, aupoc_data_ultimo_stato = CASE auldr_stato_posizione&#xA;         WHEN 1 THEN null WHEN 3 THEN auldr_data_cessazione END&#xA;FROM [dbo].[tb_aupoc_pos_contr] inner join [TB_AULDR_RAPPORTI_LD] ON  &#xA;&#x9; aupoc_codice_pk = [AULDR_CODICE_AUPOC]&#xA;WHERE [AULDR_VALIDATO_AUPOC] = 1 &#xA;AND [AULDR_FLAG_AUPOC] = 'S' &#xA;AND [aupoc_aurea_codice_pk] = 11 ;&#xA;&#xA;-- Fine" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
